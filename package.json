{
  "name": "365-daynight-vscode-theme-ext",
  "displayName": "365 DayNight VSCode Theme Ext",
  "description": "365 DayNight VSCode Themes energizes your coding experience with themes that automatically change according to the time of day, month of the year and public holidays. Designed to minimize eye strain and maximize readability, this extension makes every coding session rich and tailored to your surroundings.",
  "version": "0.7.3",
  "publisher": "mickaellherminez",
  "icon": "assets/logo-web.png",
  "engines": {
    "vscode": "^1.83.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mickaellherminez/365-daynight-vscode-theme-ext.git"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:extension.activate"
  ],
  "main": "./extension.js",
  "contributes": {
    "themes": [
      {
        "label": "365 10-october light",
        "uiTheme": "vs",
        "path": "./themes/365-10-light-theme.json"
      },
      {
        "label": "365 10-october dark",
        "uiTheme": "vs-dark",
        "path": "./themes/365-10-dark-theme.json"
      },
      {
        "label": "365 10-october high contrast",
        "uiTheme": "hc-black",
        "path": "./themes/365-10-hc-dark-theme.json"
      },
      {
        "label": "365 11-november light",
        "uiTheme": "vs",
        "path": "./themes/365-11-light-theme.json"
      },
      {
        "label": "365 11-november dark",
        "uiTheme": "vs-dark",
        "path": "./themes/365-11-dark-theme.json"
      },
      {
        "label": "365 11-november high contrast",
        "uiTheme": "hc-black",
        "path": "./themes/365-11-hc-dark-theme.json"
      }
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "node ./test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.83.0",
    "@types/mocha": "^10.0.3",
    "@types/node": "18.x",
    "eslint": "^8.52.0",
    "glob": "^10.3.10",
    "mocha": "^10.2.0",
    "typescript": "^5.2.2",
    "@vscode/test-electron": "^2.3.6"
  },
  "dependencies": {
    "punycode": "^2.1.1"
  }
}
