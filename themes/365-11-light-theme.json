  "tokenColors": [
    {
      "name": "Commentaires",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "foreground": "#8795A1",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Variables",
      "scope": [
        "variable",
        "string constant.other.placeholder",
        "variable.other.constant",
        "variable.language"
      ],
      "settings": {
        "foreground": "#4682B4"
      }
    },
    {
      "name": "Chaînes de caractères",
      "scope": [
        "string",
        "string.template",
        "punctuation.definition.string",
        "string.quoted.single",
        "string.quoted.double"
      ],
      "settings": {
        "foreground": "#D4A15D"
      }
    },
    {
      "name": "Mots-clés",
      "scope": [
        "keyword",
        "keyword.control",
        "storage.type",
        "storage.modifier"
      ],
      "settings": {
        "foreground": "#DAA520"
      }
    },
    {
      "name": "Fonctions",
      "scope": [
        "entity.name.function",
        "support.function",
        "meta.function-call",
        "variable.function"
      ],
      "settings": {
        "foreground": "#2E8B57"
      }
    },
    {
      "name": "Classes",
      "scope": [
        "entity.name.type",
        "entity.name.class",
        "entity.name.namespace",
        "entity.name.scope-resolution",
        "support.class"
      ],
      "settings": {
        "foreground": "#8B4513"
      }
    },
    {
      "name": "Méthodes",
      "scope": ["meta.method", "meta.method.declaration"],
      "settings": {
        "foreground": "#2E8B57"
      }
    },
    {
      "name": "Constantes",
      "scope": [
        "constant",
        "constant.language",
        "constant.character",
        "constant.other"
      ],
      "settings": {
        "foreground": "#FF6347"
      }
    },
    {
      "name": "Nombres",
      "scope": [
        "constant.numeric",
        "constant.numeric.integer",
        "constant.numeric.float",
        "constant.numeric.hex",
        "constant.numeric.octal"
      ],
      "settings": {
        "foreground": "#FF6347"
      }
    },
    {
      "name": "Opérateurs",
      "scope": [
        "keyword.operator",
        "keyword.operator.assignment",
        "keyword.operator.arithmetic",
        "keyword.operator.logical"
      ],
      "settings": {
        "foreground": "#8B0000"
      }
    },
    {
      "name": "Ponctuation",
      "scope": [
        "punctuation",
        "punctuation.definition",
        "punctuation.separator",
        "punctuation.terminator"
      ],
      "settings": {
        "foreground": "#35596D"
      }
    },
    {
      "name": "Balises HTML/XML",
      "scope": ["entity.name.tag", "punctuation.definition.tag"],
      "settings": {
        "foreground": "#4682B4"
      }
    },
    {
      "name": "Attributs HTML/XML",
      "scope": ["entity.other.attribute-name"],
      "settings": {
        "foreground": "#DAA520"
      }
    },
    {
      "name": "Support",
      "scope": [
        "support.type",
        "support.class",
        "support.constant",
        "support.variable"
      ],
      "settings": {
        "foreground": "#8B4513"
      }
    },
    {
      "name": "Markdown Titres",
      "scope": ["markup.heading", "punctuation.definition.heading"],
      "settings": {
        "foreground": "#4682B4",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markdown Emphase",
      "scope": ["markup.italic", "punctuation.definition.italic"],
      "settings": {
        "foreground": "#D4A15D",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Markdown Gras",
      "scope": ["markup.bold", "punctuation.definition.bold"],
      "settings": {
        "foreground": "#DAA520",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markdown Liens",
      "scope": ["markup.underline.link", "string.other.link"],
      "settings": {
        "foreground": "#FF6347"
      }
    },
    {
      "name": "Markdown Code",
      "scope": ["markup.fenced_code", "markup.inline.raw"],
      "settings": {
        "foreground": "#32CD32"
      }
    }
  ],
  "semanticTokenColors": {
    "variable.readonly": "#32CD32",
    "parameter": "#4682B4",
    "function": "#2E8B57",
    "method": "#2E8B57",
    "class": "#8B4513",
    "enum": "#DAA520",
    "interface": "#8B4513",
    "namespace": "#8795A1",
    "typeParameter": "#FF6347",
    "enumMember": "#D4A15D",
    "property": "#35596D",
    "variable.defaultLibrary": "#8B0000",
    "keyword.operator": "#8B0000",
    "comment.documentation": {
      "foreground": "#8795A1",
      "fontStyle": "italic"
    }
  }
}
