{
  "name": "Thème High Contrast Janvier - Révisé",
  "type": "dark",
  "semanticHighlighting": true,
  "colors": {
    "editor.background": "#000000",
    "editor.foreground": "#FFFFFF",
    "editorLineNumber.foreground": "#FFFFFF",
    "editorCursor.foreground": "#00FF00",
    "editor.selectionBackground": "#0050A4",
    "editor.lineHighlightBackground": "#151515",
    "editorLineNumber.activeForeground": "#00FF00",
    "editorIndentGuide.background": "#333333",
    "editorIndentGuide.activeBackground": "#555555",
    "editorWhitespace.foreground": "#333333",
    "editor.findMatchBackground": "#FFD700",
    "editor.findMatchBorder": "#FFFFFF",
    "editor.findMatchHighlightBackground": "#FFA500",
    "editor.findMatchHighlightBorder": "#FFFFFF",
    "editor.hoverHighlightBackground": "#151515",
    "editor.lineHighlightBorder": "#FFFFFF",
    "editorLink.activeForeground": "#1E90FF",
    "editor.selectionHighlightBackground": "#0050A4",
    "editor.selectionHighlightBorder": "#FFFFFF",
    "editor.wordHighlightBackground": "#444444",
    "editor.wordHighlightBorder": "#FFFFFF",
    "editor.wordHighlightStrongBackground": "#666666",
    "editor.wordHighlightStrongBorder": "#FFFFFF",
    "peekViewEditor.background": "#1A1A1A",
    "peekViewEditor.matchHighlightBackground": "#FFD700",
    "peekViewResult.background": "#1A1A1A",
    "peekViewResult.matchHighlightBackground": "#FFD700",
    "peekViewTitle.background": "#000000",
    "peekViewTitleLabel.foreground": "#FFFFFF",
    "peekViewTitleDescription.foreground": "#FFFFFF",
    "tab.activeBackground": "#000000",
    "tab.activeForeground": "#FFFFFF",
    "tab.inactiveBackground": "#1A1A1A",
    "tab.inactiveForeground": "#CCCCCC",
    "tab.border": "#FFFFFF",
    "activityBar.background": "#000000",
    "activityBar.foreground": "#FFFFFF",
    "activityBar.inactiveForeground": "#CCCCCC",
    "activityBarBadge.background": "#1E90FF",
    "activityBarBadge.foreground": "#FFFFFF",
    "sideBar.background": "#000000",
    "sideBar.foreground": "#FFFFFF",
    "sideBarTitle.foreground": "#FFFFFF",
    "sideBarSectionHeader.background": "#1A1A1A",
    "sideBarSectionHeader.foreground": "#FFFFFF",
    "statusBar.background": "#000000",
    "statusBar.foreground": "#FFFFFF",
    "statusBar.debuggingBackground": "#1A1A1A",
    "statusBar.debuggingForeground": "#FFFFFF",
    "titleBar.activeBackground": "#000000",
    "titleBar.activeForeground": "#FFFFFF",
    "titleBar.inactiveBackground": "#000000",
    "titleBar.inactiveForeground": "#CCCCCC",
    "panel.background": "#000000",
    "panel.border": "#FFFFFF",
    "panelTitle.activeForeground": "#FFFFFF",
    "panelTitle.inactiveForeground": "#CCCCCC",
    "terminal.background": "#000000",
    "terminal.foreground": "#FFFFFF",
    "terminalCursor.foreground": "#00FF00",
    "terminal.ansiBlack": "#000000",
    "terminal.ansiRed": "#FF0000",
    "terminal.ansiGreen": "#00FF00",
    "terminal.ansiYellow": "#FFFF00",
    "terminal.ansiBlue": "#1E90FF",
    "terminal.ansiMagenta": "#FF00FF",
    "terminal.ansiCyan": "#00FFFF",
    "terminal.ansiWhite": "#FFFFFF",
    "terminal.ansiBrightBlack": "#808080",
    "terminal.ansiBrightRed": "#FF4500",
    "terminal.ansiBrightGreen": "#32CD32",
    "terminal.ansiBrightYellow": "#FFD700",
    "terminal.ansiBrightBlue": "#1E90FF",
    "terminal.ansiBrightMagenta": "#EE82EE",
    "terminal.ansiBrightCyan": "#00CED1",
    "terminal.ansiBrightWhite": "#FFFFFF",
    "button.background": "#1E90FF",
    "button.foreground": "#FFFFFF",
    "button.hoverBackground": "#1C86EE",
    "dropdown.background": "#1A1A1A",
    "dropdown.foreground": "#FFFFFF",
    "input.background": "#1A1A1A",
    "input.foreground": "#FFFFFF",
    "input.placeholderForeground": "#CCCCCC",
    "scrollbarSlider.background": "#333333",
    "scrollbarSlider.hoverBackground": "#444444",
    "scrollbarSlider.activeBackground": "#555555",
    "list.activeSelectionBackground": "#1E90FF",
    "list.activeSelectionForeground": "#FFFFFF",
    "list.inactiveSelectionBackground": "#1A1A1A",
    "list.inactiveSelectionForeground": "#CCCCCC",
    "list.hoverBackground": "#333333",
    "list.hoverForeground": "#FFFFFF",
    "editorGroupHeader.tabsBackground": "#000000",
    "editorGroupHeader.tabsBorder": "#FFFFFF",
    "editorGroup.border": "#FFFFFF",
    "notification.background": "#1A1A1A",
    "notification.foreground": "#FFFFFF",
    "notification.buttonBackground": "#1E90FF",
    "notification.buttonForeground": "#FFFFFF",
    "notification.buttonHoverBackground": "#1C86EE",
    "extensionButton.prominentBackground": "#1E90FF",
    "extensionButton.prominentForeground": "#FFFFFF",
    "extensionButton.prominentHoverBackground": "#1C86EE",
    "pickerGroup.border": "#1E90FF",
    "pickerGroup.foreground": "#FFFFFF",
    "progressBar.background": "#1E90FF",
    "badge.background": "#1E90FF",
    "badge.foreground": "#FFFFFF",
    "menu.background": "#000000",
    "menu.foreground": "#FFFFFF",
    "menu.selectionBackground": "#1E90FF",
    "menu.selectionForeground": "#FFFFFF",
    "menu.separatorBackground": "#FFFFFF",
    "menubar.selectionBackground": "#1E90FF",
    "menubar.selectionForeground": "#FFFFFF"
  },
  "tokenColors": [
    {
      "name": "Commentaires",
      "scope": [
        "comment",
        "punctuation.definition.comment",
        "comment markup.link"
      ],
      "settings": {
        "foreground": "#b0b0b0",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Variables",
      "scope": [
        "variable",
        "variable.other.constant",
        "variable.language",
        "variable.parameter.function",
        "variable.parameter",
        "variable.other.readwrite",
        "variable.other.class",
        "variable.other.instance"
      ],
      "settings": {
        "foreground": "#00ffff"
      }
    },
    {
      "name": "Constantes",
      "scope": [
        "constant",
        "constant.language",
        "constant.character",
        "constant.other",
        "support.constant",
        "support.constant.edge"
      ],
      "settings": {
        "foreground": "#ffd700"
      }
    },
    {
      "name": "Chaînes de caractères",
      "scope": [
        "string",
        "string.template",
        "punctuation.definition.string",
        "string.quoted.single",
        "string.quoted.double",
        "markup.inline.raw.string.markdown"
      ],
      "settings": {
        "foreground": "#ffa500"
      }
    },
    {
      "name": "Mots-clés",
      "scope": [
        "keyword",
        "keyword.control",
        "storage.type",
        "storage.modifier",
        "keyword.operator.expression.import",
        "keyword.operator.expression.instanceof",
        "keyword.operator.new",
        "keyword.operator.ternary",
        "keyword.operator.optional",
        "keyword.operator.expression.keyof"
      ],
      "settings": {
        "foreground": "#ff4500"
      }
    },
    {
      "name": "Fonctions",
      "scope": [
        "entity.name.function",
        "support.function",
        "meta.function-call",
        "variable.function",
        "support.function.std.rust",
        "support.function.console",
        "meta.function-call.generic.python"
      ],
      "settings": {
        "foreground": "#7fff00"
      }
    },
    {
      "name": "Classes et Types",
      "scope": [
        "entity.name.type",
        "entity.name.class",
        "entity.name.namespace",
        "entity.name.scope-resolution",
        "support.class",
        "entity.other.inherited-class",
        "support.type",
        "support.type.property-name",
        "support.type.primitive.ts",
        "support.type.builtin.ts",
        "support.type.primitive.tsx",
        "support.type.builtin.tsx"
      ],
      "settings": {
        "foreground": "#ffd700"
      }
    },
    {
      "name": "Méthodes",
      "scope": [
        "meta.method",
        "meta.method.declaration",
        "meta.method.java",
        "meta.method.groovy"
      ],
      "settings": {
        "foreground": "#adff2f"
      }
    },
    {
      "name": "Nombres",
      "scope": [
        "constant.numeric",
        "constant.numeric.integer",
        "constant.numeric.float",
        "constant.numeric.hex",
        "constant.numeric.octal"
      ],
      "settings": {
        "foreground": "#00fa9a"
      }
    },
    {
      "name": "Opérateurs",
      "scope": [
        "keyword.operator",
        "keyword.operator.assignment",
        "keyword.operator.arithmetic",
        "keyword.operator.logical",
        "keyword.operator.bitwise",
        "keyword.operator.channel",
        "keyword.operator.sizeof.c",
        "keyword.operator.sizeof.cpp",
        "keyword.operator.delete"
      ],
      "settings": {
        "foreground": "#ff00ff"
      }
    },
    {
      "name": "Ponctuation",
      "scope": [
        "punctuation",
        "punctuation.definition",
        "punctuation.separator",
        "punctuation.terminator",
        "punctuation.definition.string.begin",
        "punctuation.definition.string.end",
        "punctuation.separator.key-value",
        "punctuation.separator.list.comma.css",
        "punctuation.section.block.begin.bracket.curly.cpp",
        "punctuation.section.block.end.bracket.curly.cpp"
      ],
      "settings": {
        "foreground": "#ffffff"
      }
    },
    {
      "name": "Balises HTML/XML",
      "scope": [
        "entity.name.tag",
        "punctuation.definition.tag",
        "meta.tag",
        "meta.tag.sgml",
        "markup.deleted.git_gutter"
      ],
      "settings": {
        "foreground": "#00bfff"
      }
    },
    {
      "name": "Attributs HTML/XML",
      "scope": [
        "entity.other.attribute-name",
        "entity.other.attribute-name.class.css",
        "entity.other.attribute-name.id.css"
      ],
      "settings": {
        "foreground": "#ffa500"
      }
    },
    {
      "name": "Support et modules",
      "scope": [
        "support.type",
        "support.class",
        "support.constant",
        "support.variable",
        "support.module.node",
        "support.type.object.module",
        "support.module.node",
        "entity.name.type.module"
      ],
      "settings": {
        "foreground": "#daa520"
      }
    },
    {
      "name": "Markdown Titres",
      "scope": [
        "markup.heading",
        "punctuation.definition.heading",
        "entity.name.section.markdown",
        "markup.heading.setext",
        "markup.heading.setext.1.markdown",
        "markup.heading.setext.2.markdown"
      ],
      "settings": {
        "foreground": "#ff4500",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markdown Emphase",
      "scope": [
        "markup.italic",
        "punctuation.definition.italic",
        "emphasis md",
        "markup.italic.markdown"
      ],
      "settings": {
        "foreground": "#ffa500",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Markdown Gras",
      "scope": [
        "markup.bold",
        "punctuation.definition.bold",
        "punctuation.definition.bold.markdown"
      ],
      "settings": {
        "foreground": "#ffd700",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markdown Liens",
      "scope": [
        "markup.underline.link",
        "string.other.link",
        "markup.underline.link.markdown",
        "markup.underline.link.image.markdown"
      ],
      "settings": {
        "foreground": "#1e90ff",
        "fontStyle": "underline"
      }
    },
    {
      "name": "Markdown Code",
      "scope": [
        "markup.fenced_code",
        "markup.inline.raw",
        "markup.inline.raw.markdown",
        "markup.inline.raw.string.markdown"
      ],
      "settings": {
        "foreground": "#00ff00"
      }
    },
    {
      "name": "Markdown Listes",
      "scope": [
        "markup.list",
        "punctuation.definition.list",
        "punctuation.definition.list.begin.markdown",
        "punctuation.definition.list.markdown"
      ],
      "settings": {
        "foreground": "#00ffff"
      }
    },
    {
      "name": "Markdown Citations",
      "scope": [
        "markup.quote",
        "markup.quote.markdown"
      ],
      "settings": {
        "foreground": "#b0b0b0",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Caractères spéciaux",
      "scope": [
        "constant.character.escape",
        "constant.character.escape.markdown"
      ],
      "settings": {
        "foreground": "#ff00ff"
      }
    },
    {
      "name": "Stockage",
      "scope": [
        "storage",
        "token.storage",
        "token.storage.type.java"
      ],
      "settings": {
        "foreground": "#ff4500"
      }
    },
    {
      "name": "Entités",
      "scope": [
        "entity.other.inherited-class",
        "entity.global.clojure",
        "meta.definition.class.inherited.classes.groovy"
      ],
      "settings": {
        "foreground": "#ffd700"
      }
    },
    {
      "name": "Propriétés JSON",
      "scope": [
        "support.type.property-name.json",
        "support.type.property-name.json punctuation"
      ],
      "settings": {
        "foreground": "#ffa500"
      }
    },
    {
      "name": "Expressions régulières",
      "scope": [
        "string.regexp",
        "constant.other.character-class.regexp",
        "keyword.operator.quantifier.regexp"
      ],
      "settings": {
        "foreground": "#ff69b4"
      }
    },
    {
      "name": "Invalide",
      "scope": [
        "invalid",
        "invalid.illegal",
        "invalid.broken",
        "invalid.deprecated",
        "invalid.unimplemented"
      ],
      "settings": {
        "foreground": "#ff4500",
        "fontStyle": "underline"
      }
    },
    {
      "name": "Déprécié",
      "scope": [
        "invalid.deprecated"
      ],
      "settings": {
        "foreground": "#ffd700",
        "fontStyle": "strikethrough"
      }
    },
    {
      "name": "JSON Punctuation for Property Name",
      "scope": [
        "support.type.property-name.json punctuation"
      ],
      "settings": {
        "foreground": "#ffa500"
      }
    },
    {
      "name": "JSON String Value",
      "scope": [
        "source.json meta.structure.dictionary.json > value.json > string.quoted.json",
        "source.json meta.structure.array.json > value.json > string.quoted.json"
      ],
      "settings": {
        "foreground": "#ffa500"
      }
    },
    {
      "name": "JSON Key",
      "scope": [
        "source.json meta.structure.dictionary.json > string.quoted.json"
      ],
      "settings": {
        "foreground": "#00ffff"
      }
    },
    {
      "name": "JSON Punctuation",
      "scope": [
        "source.json meta.structure.dictionary.json > string.quoted.json > punctuation.string",
        "source.json meta.structure.dictionary.json > value.json > string.quoted.json > punctuation",
        "source.json meta.structure.array.json > value.json > string.quoted.json > punctuation"
      ],
      "settings": {
        "foreground": "#ffd700"
      }
    }
  ]
}