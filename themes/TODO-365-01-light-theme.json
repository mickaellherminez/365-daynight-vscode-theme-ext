{
  "name": "Custom Light Theme",
  "type": "light",
  "colors": {
    "editor.background": "#FFFFFF",
    "editor.foreground": "#424242",
    "editorLineNumber.foreground": "#A0C1D1",
    "editor.selectionBackground": "#BDD6EE",
    "editor.comment": "#7F848E",
    "editor.keyword": "#003366",
    "editor.variable": "#00563F",
    "editor.string": "#FFA07A",
    "sideBar.background": "#F3F3F3",
    "sideBarTitle.foreground": "#6F6F6F",
    "activityBar.background": "#2c2c2c",
    "activityBar.foreground": "#ffffff",
    "activityBar.inactiveForeground": "#ffffff66",
    "activityBarBadge.background": "#007acc",
    "activityBarBadge.foreground": "#ffffff",
    "statusBar.background": "#007acc",
    "statusBar.foreground": "#ffffff",
    "statusBarItem.hoverBackground": "#ffffff1f",
    "titleBar.activeBackground": "#dddddd",
    "titleBar.activeForeground": "#333333",
    "titleBar.inactiveBackground": "#dddddd99",
    "titleBar.inactiveForeground": "#33333399",
    "button.background": "#007acc",
    "button.foreground": "#ffffff",
    "dropdown.background": "#ffffff",
    "dropdown.foreground": "#616161",
    "input.background": "#ffffff",
    "input.foreground": "#616161",
    "scrollbar.shadow": "#dddddd",
    "scrollbarSlider.background": "#64646466",
    "scrollbarSlider.hoverBackground": "#646464b3",
    "scrollbarSlider.activeBackground": "#00000099",
    "badge.background": "#c4c4c4",
    "badge.foreground": "#333333",
    "progressBar.background": "#0e70c0",
    "list.hoverForeground": "#616161",
    "list.inactiveSelectionBackground": "#e4e6f1",
    "list.inactiveSelectionForeground": "#616161",
    "list.activeSelectionBackground": "#0060c0",
    "list.activeSelectionForeground": "#ffffff",
    "list.focusBackground": "#d6ebff",
    "list.focusForeground": "#616161",
    "list.highlightForeground": "#0066bf",
    "list.errorForeground": "#B00",
    "list.warningForeground": "#B5BA00",
    "editorWarning.foreground": "#e9a700",
    "editorError.foreground": "#e51400",
    "editorInfo.foreground": "#75beff",
    "editorHint.foreground": "#6c6c6c",
    "peekView.border": "#007acc",
    "peekViewEditor.background": "#f2f8fc",
    "peekViewResult.background": "#f3f3f3"
  },
  "tokenColors": [
    {
      "scope": ["comment", "punctuation.definition.comment", "string.comment"],
      "settings": {
        "foreground": "#7F848E",
        "fontStyle": "italic"
      }
    },
    {
      "scope": "constant",
      "settings": {
        "foreground": "#00563F"
      }
    },
    {
      "scope": "entity.name",
      "settings": {
        "foreground": "#003366"
      }
    },
    {
      "scope": "keyword",
      "settings": {
        "foreground": "#003366",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "storage",
      "settings": {
        "foreground": "#00563F"
      }
    },
    {
      "scope": "string",
      "settings": {
        "foreground": "#FFA07A"
      }
    },
    {
      "scope": "support",
      "settings": {
        "foreground": "#003366"
      }
    },
    {
      "scope": "variable",
      "settings": {
        "foreground": "#00563F"
      }
    },
    {
      "scope": "variable.parameter",
      "settings": {
        "foreground": "#424242"
      }
    },
    {
      "scope": ["entity.name.tag", "entity.other.attribute-name"],
      "settings": {
        "foreground": "#00563F"
      }
    },
    {
      "scope": "meta.directive",
      "settings": {
        "foreground": "#003366"
      }
    },
    {
      "scope": "punctuation.definition.tag",
      "settings": {
        "foreground": "#003366"
      }
    },
    {
      "scope": "meta.diff.header",
      "settings": {
        "foreground": "#424242"
      }
    },
    {
      "scope": ["markup.deleted", "markup.inserted", "markup.changed"],
      "settings": {
        "foreground": "#424242"
      }
    },
    {
      "scope": "markup.italic",
      "settings": {
        "foreground": "#7F848E",
        "fontStyle": "italic"
      }
    },
    {
      "scope": "markup.bold",
      "settings": {
        "foreground": "#7F848E",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "markup.underline",
      "settings": {
        "foreground": "#7F848E",
        "fontStyle": "underline"
      }
    },
    {
      "scope": "markup.heading",
      "settings": {
        "foreground": "#003366",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "markup.quote",
      "settings": {
        "foreground": "#424242"
      }
    },
    {
      "scope": "markup.list",
      "settings": {
        "foreground": "#424242"
      }
    }

  ]
}
