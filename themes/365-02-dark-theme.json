  "tokenColors": [
    {
      "scope": ["comment", "punctuation.definition.comment", "string.comment"],
      "settings": {
        "foreground": "#7F848E",
        "fontStyle": "italic"
      }
    },
    {
      "scope": "constant",
      "settings": {
        "foreground": "#FFEA00" // Jaune
      }
    },
    {
      "scope": "entity.name",
      "settings": {
        "foreground": "#800080" // Violet
      }
    },
    {
      "scope": "keyword",
      "settings": {
        "foreground": "#800080", // Violet
        "fontStyle": "bold"
      }
    },
    {
      "scope": "storage",
      "settings": {
        "foreground": "#FFEA00" // Jaune
      }
    },
    {
      "scope": "string",
      "settings": {
        "foreground": "#FFA07A"
      }
    },
    {
      "scope": "support",
      "settings": {
        "foreground": "#800080" // Violet
      }
    },
    {
      "scope": "variable",
      "settings": {
        "foreground": "#FFEA00" // Jaune
      }
    },
    {
      "scope": "variable.parameter",
      "settings": {
        "foreground": "#424242"
      }
    },
    {
      "scope": ["entity.name.tag", "entity.other.attribute-name"],
      "settings": {
        "foreground": "#FFEA00" // Jaune
      }
    },
    {
      "scope": "meta.directive",
      "settings": {
        "foreground": "#800080" // Violet
      }
    },
    {
      "scope": "punctuation.definition.tag",
      "settings": {
        "foreground": "#800080" // Violet
      }
    },
    {
      "scope": "meta.diff.header",
      "settings": {
        "foreground": "#424242"
      }
    },
    {
      "scope": ["markup.deleted", "markup.inserted", "markup.changed"],
      "settings": {
        "foreground": "#424242"
      }
    },
    {
      "scope": "markup.italic",
      "settings": {
        "foreground": "#7F848E",
        "fontStyle": "italic"
      }
    },
    {
      "scope": "markup.bold",
      "settings": {
        "foreground": "#7F848E",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "markup.underline",
      "settings": {
        "foreground": "#7F848E",
        "fontStyle": "underline"
      }
    },
    {
      "scope": "markup.heading",
      "settings": {
        "foreground": "#800080", // Violet
        "fontStyle": "bold"
      }
    },
    {
      "scope": "markup.quote",
      "settings": {
        "foreground": "#424242"
      }
    },
    {
      "scope": "markup.list",
      "settings": {
        "foreground": "#424242"
      }
    }
  ]
}
