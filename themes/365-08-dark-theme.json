  "tokenColors": [
    {
      "name": "Comments",
      "scope": [
        "comment",
        "punctuation.definition.comment"
      ],
      "settings": {
        "foreground": "#64748B"
      }
    },
    {
      "name": "Keywords",
      "scope": [
        "keyword",
        "keyword.control",
        "storage.type",
        "storage.modifier"
      ],
      "settings": {
        "foreground": "#FACC15"
      }
    },
    {
      "name": "Variables",
      "scope": [
        "variable",
        "string constant.other.placeholder",
        "variable.other.constant",
        "variable.language"
      ],
      "settings": {
        "foreground": "#60A5FA"
      }
    },
    {
      "name": "Strings",
      "scope": [
        "string",
        "string.template",
        "punctuation.definition.string",
        "string.quoted.single",
        "string.quoted.double"
      ],
      "settings": {
        "foreground": "#22C55E"
      }
    },
    {
      "name": "Functions",
      "scope": [
        "entity.name.function",
        "support.function",
        "meta.function-call",
        "variable.function"
      ],
      "settings": {
        "foreground": "#3B82F6"
      }
    },
    {
      "name": "Classes",
      "scope": [
        "entity.name.type",
        "entity.name.class",
        "entity.name.namespace",
        "entity.name.scope-resolution",
        "support.class"
      ],
      "settings": {
        "foreground": "#C084FC"
      }
    },
    {
      "name": "Numbers",
      "scope": [
        "constant.numeric",
        "constant.numeric.integer",
        "constant.numeric.float",
        "constant.numeric.hex",
        "constant.numeric.octal"
      ],
      "settings": {
        "foreground": "#FACC15"
      }
    },
    {
      "name": "Operators",
      "scope": [
        "keyword.operator",
        "keyword.operator.assignment",
        "keyword.operator.arithmetic",
        "keyword.operator.logical"
      ],
      "settings": {
        "foreground": "#EF4444"
      }
    },
    {
      "name": "Constants",
      "scope": [
        "constant",
        "constant.language",
        "constant.character",
        "constant.other"
      ],
      "settings": {
        "foreground": "#FACC15"
      }
    },
    {
      "name": "Punctuation",
      "scope": [
        "punctuation",
        "punctuation.definition",
        "punctuation.separator",
        "punctuation.terminator"
      ],
      "settings": {
        "foreground": "#F1F5F9"
      }
    },
    {
      "name": "HTML/XML Tags",
      "scope": [
        "entity.name.tag",
        "punctuation.definition.tag"
      ],
      "settings": {
        "foreground": "#3B82F6"
      }
    },
    {
      "name": "HTML/XML Attributes",
      "scope": [
        "entity.other.attribute-name"
      ],
      "settings": {
        "foreground": "#FACC15"
      }
    },
    {
      "name": "Support",
      "scope": [
        "support.type",
        "support.class",
        "support.constant",
        "support.variable"
      ],
      "settings": {
        "foreground": "#60A5FA"
      }
    },
    {
      "name": "Markdown Bold",
      "scope": [
        "markup.bold",
        "punctuation.definition.bold"
      ],
      "settings": {
        "foreground": "#EF4444",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markdown Links",
      "scope": [
        "markup.underline.link",
        "string.other.link"
      ],
      "settings": {
        "foreground": "#3B82F6",
        "fontStyle": "underline"
      }
    },
    {
      "name": "Markdown Code",
      "scope": [
        "markup.fenced_code",
        "markup.inline.raw"
      ],
      "settings": {
        "foreground": "#FACC15"
      }
    },
    {
      "name": "Markup Lists",
      "scope": [
        "markup.list.unnumbered",
        "markup.list.numbered"
      ],
      "settings": {
        "foreground": "#F1F5F9"
      }
    },
    {
      "name": "Markup Quotes",
      "scope": [
        "markup.quote"
      ],
      "settings": {
        "foreground": "#22C55E",
        "fontStyle": "italic"
      }
    },
    {
      "name": "RegExp String",
      "scope": [
        "string.regexp",
        "string.regexp keyword.other"
      ],
      "settings": {
        "foreground": "#06B6D4"
      }
    },
    {
      "name": "Escape Characters",
      "scope": [
        "constant.character.escape"
      ],
      "settings": {
        "foreground": "#FFA500"
      }
    },
    {
      "name": "URL",
      "scope": [
        "meta.tag.inline.any.html",
        "*url*",
        "*link*",
        "*uri*"
      ],
      "settings": {
        "fontStyle": "underline"
      }
    },
    {
      "name": "Changed",
      "scope": [
        "markup.changed",
        "meta.diff.header.git",
        "meta.diff.header.from-file",
        "meta.diff.header.to-file"
      ],
      "settings": {
        "foreground": "#3B82F6",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Deleted",
      "scope": [
        "markup.deleted"
      ],
      "settings": {
        "foreground": "#EF4444"
      }
    },
    {
      "name": "Inserted",
      "scope": [
        "markup.inserted"
      ],
      "settings": {
        "foreground": "#22C55E"
      }
    },
    {
      "name": "Invalid",
      "scope": [
        "invalid",
        "invalid.illegal"
      ],
      "settings": {
        "foreground": "#EF4444",
        "fontStyle": "underline wavy"
      }
    },
    {
      "name": "Deprecated",
      "scope": [
        "invalid.deprecated"
      ],
      "settings": {
        "foreground": "#FACC15",
        "fontStyle": "underline wavy"
      }
    },
    {
      "name": "Diff Header",
      "scope": [
        "meta.diff.header"
      ],
      "settings": {
        "foreground": "#3B82F6"
      }
    },
    {
      "name": "Diff Deleted",
      "scope": [
        "markup.deleted.diff"
      ],
      "settings": {
        "foreground": "#EF4444"
      }
    },
    {
      "name": "Diff Inserted",
      "scope": [
        "markup.inserted.diff"
      ],
      "settings": {
        "foreground": "#22C55E"
      }
    },
    {
      "name": "Diff Changed",
      "scope": [
        "markup.changed.diff"
      ],
      "settings": {
        "foreground": "#FACC15"
      }
    },
    {
      "name": "Méthodes",
      "scope": [
        "meta.method",
        "meta.method.declaration"
      ],
      "settings": {
        "foreground": "#3B82F6"
      }
    },
    {
      "name": "JSON Keys",
      "scope": "support.type.property-name.json",
      "settings": {
        "foreground": "#FACC15"
      }
    },
    {
      "name": "YAML Keys",
      "scope": "entity.name.tag.yaml",
      "settings": {
        "foreground": "#FACC15"
      }
    },
    {
      "name": "Entités",
      "scope": [
        "entity.other.inherited-class"
      ],
      "settings": {
        "foreground": "#C084FC"
      }
    },
    {
      "name": "Balises Markdown",
      "scope": [
        "meta.tag",
        "meta.tag.markdown"
      ],
      "settings": {
        "foreground": "#FACC15"
      }
    }
  ],
  "_accessibilityNote": "Palette sombre optimisée pour un contraste AA/AAA (≥4.5:1, idéalement ≥7:1) sur tous les éléments principaux et interactifs. Les couleurs d'alerte sont différenciées et testées pour éviter toute confusion daltonienne (protanopie, deuteranopie, tritanopie). Jamais de couleur seule : toujours icône, bordure ou motif en plus. Focus clavier visible et contrasté. Palette cohérente sur toutes les variantes. Documentation à jour après chaque changement. Voir guide accessibilité du projet."
}