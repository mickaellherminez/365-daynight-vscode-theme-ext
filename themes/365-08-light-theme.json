  "tokenColors": [
    {
      "name": "Comments",
      "scope": [
        "comment",
        "punctuation.definition.comment"
      ],
      "settings": {
        "foreground": "#9E9E9E"
      }
    },
    {
      "name": "Keywords",
      "scope": [
        "keyword",
        "keyword.control",
        "storage.type",
        "storage.modifier"
      ],
      "settings": {
        "foreground": "#FF6F00"
      }
    },
    {
      "name": "Variables",
      "scope": [
        "variable",
        "string constant.other.placeholder",
        "variable.other.constant",
        "variable.language"
      ],
      "settings": {
        "foreground": "#00ACC1"
      }
    },
    {
      "name": "Strings",
      "scope": [
        "string",
        "string.template",
        "punctuation.definition.string",
        "string.quoted.single",
        "string.quoted.double"
      ],
      "settings": {
        "foreground": "#4CAF50"
      }
    },
    {
      "name": "Functions",
      "scope": [
        "entity.name.function",
        "support.function",
        "meta.function-call",
        "variable.function"
      ],
      "settings": {
        "foreground": "#1976D2"
      }
    },
    {
      "name": "Classes",
      "scope": [
        "entity.name.type",
        "entity.name.class",
        "entity.name.namespace",
        "entity.name.scope-resolution",
        "support.class"
      ],
      "settings": {
        "foreground": "#FF3D00"
      }
    },
    {
      "name": "Markdown Emphasis",
      "scope": [
        "markup.italic",
        "punctuation.definition.italic"
      ],
      "settings": {
        "foreground": "#4CAF50"
      }
    },
    {
      "name": "Markdown Headers",
      "scope": [
        "markup.heading",
        "punctuation.definition.heading"
      ],
      "settings": {
        "foreground": "#1976D2",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Constants",
      "scope": [
        "constant",
        "constant.language",
        "constant.character",
        "constant.other"
      ],
      "settings": {
        "foreground": "#FFC107"
      }
    },
    {
      "name": "Numbers",
      "scope": [
        "constant.numeric",
        "constant.numeric.integer",
        "constant.numeric.float",
        "constant.numeric.hex",
        "constant.numeric.octal"
      ],
      "settings": {
        "foreground": "#8E24AA"
      }
    },
    {
      "name": "Operators",
      "scope": [
        "keyword.operator",
        "keyword.operator.assignment",
        "keyword.operator.arithmetic",
        "keyword.operator.logical"
      ],
      "settings": {
        "foreground": "#FF6F00"
      }
    },
    {
      "name": "Punctuation",
      "scope": [
        "punctuation",
        "punctuation.definition",
        "punctuation.separator",
        "punctuation.terminator"
      ],
      "settings": {
        "foreground": "#1A237E"
      }
    },
    {
      "name": "HTML/XML Tags",
      "scope": [
        "entity.name.tag",
        "punctuation.definition.tag"
      ],
      "settings": {
        "foreground": "#1976D2"
      }
    },
    {
      "name": "HTML/XML Attributes",
      "scope": [
        "entity.other.attribute-name"
      ],
      "settings": {
        "foreground": "#4CAF50"
      }
    },
    {
      "name": "Support",
      "scope": [
        "support.type",
        "support.class",
        "support.constant",
        "support.variable"
      ],
      "settings": {
        "foreground": "#00ACC1"
      }
    },
    {
      "name": "Markdown Bold",
      "scope": [
        "markup.bold",
        "punctuation.definition.bold"
      ],
      "settings": {
        "foreground": "#FF3D00",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markdown Links",
      "scope": [
        "markup.underline.link",
        "string.other.link"
      ],
      "settings": {
        "foreground": "#1976D2",
        "fontStyle": "underline"
      }
    },
    {
      "name": "Markdown Code",
      "scope": [
        "markup.fenced_code",
        "markup.inline.raw"
      ],
      "settings": {
        "foreground": "#FFC107"
      }
    },
    {
      "name": "Markup Lists",
      "scope": [
        "markup.list.unnumbered",
        "markup.list.numbered"
      ],
      "settings": {
        "foreground": "#1A237E"
      }
    },
    {
      "name": "Markup Quotes",
      "scope": [
        "markup.quote"
      ],
      "settings": {
        "foreground": "#4CAF50",
        "fontStyle": "italic"
      }
    },
    {
      "name": "RegExp String",
      "scope": [
        "string.regexp",
        "string.regexp keyword.other"
      ],
      "settings": {
        "foreground": "#00ACC1"
      }
    },
    {
      "name": "Escape Characters",
      "scope": [
        "constant.character.escape"
      ],
      "settings": {
        "foreground": "#FF3D00"
      }
    },
    {
      "name": "URL",
      "scope": [
        "meta.tag.inline.any.html",
        "*url*",
        "*link*",
        "*uri*"
      ],
      "settings": {
        "fontStyle": "underline"
      }
    },
    {
      "name": "Changed",
      "scope": [
        "markup.changed",
        "meta.diff.header.git",
        "meta.diff.header.from-file",
        "meta.diff.header.to-file"
      ],
      "settings": {
        "foreground": "#1976D2",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Deleted",
      "scope": [
        "markup.deleted"
      ],
      "settings": {
        "foreground": "#FF3D00"
      }
    },
    {
      "name": "Inserted",
      "scope": [
        "markup.inserted"
      ],
      "settings": {
        "foreground": "#4CAF50"
      }
    },
    {
      "name": "Invalid",
      "scope": [
        "invalid",
        "invalid.illegal"
      ],
      "settings": {
        "foreground": "#FF3D00",
        "fontStyle": "underline wavy"
      }
    },
    {
      "name": "Deprecated",
      "scope": [
        "invalid.deprecated"
      ],
      "settings": {
        "foreground": "#FFC107",
        "fontStyle": "underline wavy"
      }
    },
    {
      "name": "Diff Header",
      "scope": [
        "meta.diff.header"
      ],
      "settings": {
        "foreground": "#1976D2"
      }
    },
    {
      "name": "Diff Deleted",
      "scope": [
        "markup.deleted.diff"
      ],
      "settings": {
        "foreground": "#FF3D00"
      }
    },
    {
      "name": "Diff Inserted",
      "scope": [
        "markup.inserted.diff"
      ],
      "settings": {
        "foreground": "#4CAF50"
      }
    },
    {
      "name": "Diff Changed",
      "scope": [
        "markup.changed.diff"
      ],
      "settings": {
        "foreground": "#FFC107"
      }
    },
    {
      "name": "Méthodes",
      "scope": [
        "meta.method",
        "meta.method.declaration"
      ],
      "settings": {
        "foreground": "#1976D2"
      }
    },
    {
      "name": "JSON Keys",
      "scope": "support.type.property-name.json",
      "settings": {
        "foreground": "#FFC107"
      }
    },
    {
      "name": "YAML Keys",
      "scope": "entity.name.tag.yaml",
      "settings": {
        "foreground": "#FFC107"
      }
    },
    {
      "name": "Entités",
      "scope": [
        "entity.other.inherited-class"
      ],
      "settings": {
        "foreground": "#FF3D00"
      }
    }
  ],
  "_accessibilityNote": "Palette claire optimisée pour un contraste AA/AAA (≥4.5:1, idéalement ≥7:1) sur tous les éléments principaux et interactifs. Les couleurs d'alerte sont différenciées et testées pour éviter toute confusion daltonienne (protanopie, deuteranopie, tritanopie). Jamais de couleur seule : toujours icône, bordure ou motif en plus. Focus clavier visible et contrasté. Palette cohérente sur toutes les variantes. Documentation à jour après chaque changement. Voir guide accessibilité du projet."
}