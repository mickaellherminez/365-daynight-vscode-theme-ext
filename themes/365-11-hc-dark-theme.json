  "tokenColors": [
    {
      "name": "Commentaires",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "foreground": "#B3B3B3",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Variables",
      "scope": [
        "variable",
        "string constant.other.placeholder",
        "variable.other.constant",
        "variable.language"
      ],
      "settings": {
        "foreground": "#FFCC00"
      }
    },
    {
      "name": "Chaînes de caractères",
      "scope": [
        "string",
        "string.template",
        "punctuation.definition.string",
        "string.quoted.single",
        "string.quoted.double"
      ],
      "settings": {
        "foreground": "#FF3300"
      }
    },
    {
      "name": "Mots-clés",
      "scope": [
        "keyword",
        "keyword.control",
        "storage.type",
        "storage.modifier"
      ],
      "settings": {
        "foreground": "#FF6600"
      }
    },
    {
      "name": "Fonctions",
      "scope": [
        "entity.name.function",
        "support.function",
        "meta.function-call",
        "variable.function"
      ],
      "settings": {
        "foreground": "#33FF33"
      }
    },
    {
      "name": "Classes",
      "scope": [
        "entity.name.type",
        "entity.name.class",
        "entity.name.namespace",
        "entity.name.scope-resolution",
        "support.class"
      ],
      "settings": {
        "foreground": "#8B4513"
      }
    },
    {
      "name": "Méthodes",
      "scope": ["meta.method", "meta.method.declaration"],
      "settings": {
        "foreground": "#33FF33"
      }
    },
    {
      "name": "Constantes",
      "scope": [
        "constant",
        "constant.language",
        "constant.character",
        "constant.other"
      ],
      "settings": {
        "foreground": "#FFCC00"
      }
    },
    {
      "name": "Nombres",
      "scope": [
        "constant.numeric",
        "constant.numeric.integer",
        "constant.numeric.float",
        "constant.numeric.hex",
        "constant.numeric.octal"
      ],
      "settings": {
        "foreground": "#FFCC00"
      }
    },
    {
      "name": "Opérateurs",
      "scope": [
        "keyword.operator",
        "keyword.operator.assignment",
        "keyword.operator.arithmetic",
        "keyword.operator.logical"
      ],
      "settings": {
        "foreground": "#FF3300"
      }
    },
    {
      "name": "Ponctuation",
      "scope": [
        "punctuation",
        "punctuation.definition",
        "punctuation.separator",
        "punctuation.terminator"
      ],
      "settings": {
        "foreground": "#FFFFFF"
      }
    },
    {
      "name": "Balises HTML/XML",
      "scope": ["entity.name.tag", "punctuation.definition.tag"],
      "settings": {
        "foreground": "#FFCC00"
      }
    },
    {
      "name": "Attributs HTML/XML",
      "scope": ["entity.other.attribute-name"],
      "settings": {
        "foreground": "#FF6600"
      }
    },
    {
      "name": "Support",
      "scope": [
        "support.type",
        "support.class",
        "support.constant",
        "support.variable"
      ],
      "settings": {
        "foreground": "#8B4513"
      }
    },
    {
      "name": "Markdown Titres",
      "scope": ["markup.heading", "punctuation.definition.heading"],
      "settings": {
        "foreground": "#FFCC00",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markdown Emphase",
      "scope": ["markup.italic", "punctuation.definition.italic"],
      "settings": {
        "foreground": "#FF3300",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Markdown Gras",
      "scope": ["markup.bold", "punctuation.definition.bold"],
      "settings": {
        "foreground": "#FF6600",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markdown Liens",
      "scope": ["markup.underline.link", "string.other.link"],
      "settings": {
        "foreground": "#FFCC00"
      }
    },
    {
      "name": "Markdown Code",
      "scope": ["markup.fenced_code", "markup.inline.raw"],
      "settings": {
        "foreground": "#00FF00"
      }
    }
  ],
  "semanticTokenColors": {
    "variable.readonly": "#00FF00",
    "parameter": "#FFCC00",
    "function": "#33FF33",
    "method": "#33FF33",
    "class": "#8B4513",
    "enum": "#FF6600",
    "interface": "#8B4513",
    "namespace": "#B3B3B3",
    "typeParameter": "#FFCC00",
    "enumMember": "#FFCC00",
    "property": "#FFFFFF",
    "variable.defaultLibrary": "#FF3300",
    "keyword.operator": "#FF3300",
    "comment.documentation": {
      "foreground": "#B3B3B3",
      "fontStyle": "italic"
    }
  }
}
