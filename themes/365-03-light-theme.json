  "tokenColors": [
    {
      "name": "Commentaires",
      "scope": [
        "comment",
        "punctuation.definition.comment"
      ],
      "settings": {
        "foreground": "#7E8CA1"
      }
    },
    {
      "name": "Mots-clés",
      "scope": [
        "keyword",
        "keyword.control",
        "storage.type",
        "storage.modifier"
      ],
      "settings": {
        "foreground": "#FF8800"
      }
    },
    {
      "name": "Variables",
      "scope": [
        "variable",
        "string constant.other.placeholder",
        "variable.other.constant",
        "variable.language"
      ],
      "settings": {
        "foreground": "#00BFFF"
      }
    },
    {
      "name": "Chaînes",
      "scope": [
        "string",
        "string.template",
        "punctuation.definition.string",
        "string.quoted.single",
        "string.quoted.double"
      ],
      "settings": {
        "foreground": "#32CD32"
      }
    },
    {
      "name": "Fonctions",
      "scope": [
        "entity.name.function",
        "support.function",
        "meta.function-call",
        "variable.function"
      ],
      "settings": {
        "foreground": "#00BFFF"
      }
    },
    {
      "name": "Classes",
      "scope": [
        "entity.name.type",
        "entity.name.class",
        "entity.name.namespace",
        "entity.name.scope-resolution",
        "support.class"
      ],
      "settings": {
        "foreground": "#FFA500"
      }
    },
    {
      "name": "Constantes",
      "scope": [
        "constant",
        "constant.language",
        "constant.character",
        "constant.other"
      ],
      "settings": {
        "foreground": "#FF6347"
      }
    },
    {
      "name": "Nombres",
      "scope": [
        "constant.numeric",
        "constant.numeric.integer",
        "constant.numeric.float",
        "constant.numeric.hex",
        "constant.numeric.octal"
      ],
      "settings": {
        "foreground": "#FFD700"
      }
    },
    {
      "name": "Opérateurs",
      "scope": [
        "keyword.operator",
        "keyword.operator.assignment",
        "keyword.operator.arithmetic",
        "keyword.operator.logical"
      ],
      "settings": {
        "foreground": "#FF4500"
      }
    },
    {
      "name": "Ponctuation",
      "scope": [
        "punctuation",
        "punctuation.definition",
        "punctuation.separator",
        "punctuation.terminator"
      ],
      "settings": {
        "foreground": "#1A1A1A"
      }
    },
    {
      "name": "Balises HTML/XML",
      "scope": [
        "entity.name.tag",
        "punctuation.definition.tag"
      ],
      "settings": {
        "foreground": "#00BFFF"
      }
    },
    {
      "name": "Attributs HTML/XML",
      "scope": [
        "entity.other.attribute-name"
      ],
      "settings": {
        "foreground": "#FFD700"
      }
    },
    {
      "name": "Support",
      "scope": [
        "support.type",
        "support.class",
        "support.constant",
        "support.variable"
      ],
      "settings": {
        "foreground": "#DAA520"
      }
    },
    {
      "name": "Titres Markdown",
      "scope": [
        "markup.heading",
        "punctuation.definition.heading"
      ],
      "settings": {
        "foreground": "#FFA500",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Emphase Markdown",
      "scope": [
        "markup.italic",
        "punctuation.definition.italic"
      ],
      "settings": {
        "foreground": "#FFD700"
      }
    },
    {
      "name": "Gras Markdown",
      "scope": [
        "markup.bold",
        "punctuation.definition.bold"
      ],
      "settings": {
        "foreground": "#FFA500",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Liens Markdown",
      "scope": [
        "markup.underline.link",
        "string.other.link"
      ],
      "settings": {
        "foreground": "#00BFFF",
        "fontStyle": "underline"
      }
    },
    {
      "name": "Code Markdown",
      "scope": [
        "markup.fenced_code",
        "markup.inline.raw"
      ],
      "settings": {
        "foreground": "#32CD32"
      }
    },
    {
      "name": "Caractères d'échappement",
      "scope": [
        "constant.character.escape"
      ],
      "settings": {
        "foreground": "#FF6347"
      }
    },
    {
      "name": "Stockage",
      "scope": [
        "storage"
      ],
      "settings": {
        "foreground": "#FFD700"
      }
    },
    {
      "name": "Entités",
      "scope": [
        "entity.other.inherited-class"
      ],
      "settings": {
        "foreground": "#FFA500"
      }
    },
    {
      "name": "Clés JSON",
      "scope": "support.type.property-name.json",
      "settings": {
        "foreground": "#FFD700"
      }
    },
    {
      "name": "Classes CSS",
      "scope": "entity.other.attribute-name.class.css",
      "settings": {
        "foreground": "#00BFFF"
      }
    },
    {
      "name": "IDs CSS",
      "scope": "entity.other.attribute-name.id.css",
      "settings": {
        "foreground": "#32CD32"
      }
    },
    {
      "name": "Expressions régulières",
      "scope": "string.regexp",
      "settings": {
        "foreground": "#8A2BE2"
      }
    },
    {
      "name": "Constantes de langage",
      "scope": "variable.language",
      "settings": {
        "foreground": "#FFD700"
      }
    },
    {
      "name": "Fonctions de bibliothèque",
      "scope": "support.function",
      "settings": {
        "foreground": "#00BFFF"
      }
    },
    {
      "name": "Constantes de bibliothèque",
      "scope": "support.constant",
      "settings": {
        "foreground": "#DAA520"
      }
    }
  ],
