{
  "$schema": "vscode://schemas/color-theme",
  "name": "Christmas Light",
  "type": "light",
  "semanticHighlighting": true,
  "colors": {
    "editor.background": "#ffffff",
    "editor.foreground": "#000000",
    "activityBar.background": "#d3f8e2",
    "sideBar.background": "#fffafa",
    "statusBar.background": "#ff6347",
    "titleBar.activeBackground": "#ffd700",
    "button.background": "#008000",
    "textLink.foreground": "#ff0000",
    "dropdown.background": "#f0f0f0",
    "input.background": "#f0f0f0",
    "list.activeSelectionBackground": "#d3f8e2",
    "list.hoverBackground": "#ffd700",
    "badge.background": "#008000",
    "progressBar.background": "#ff6347",
    "errorForeground": "#ff4500"
  },
  "tokenColors": [
    {
      "scope": [
        "comment",
        "punctuation.definition.comment"
      ],
      "settings": {
        "foreground": "#6272a4"
      }
    },
    {
      "scope": [
        "string",
        "constant.other.symbol"
      ],
      "settings": {
        "foreground": "#50fa7b"
      }
    },
    {
      "scope": [
        "variable",
        "variable.language"
      ],
      "settings": {
        "foreground": "#8be9fd"
      }
    },
    {
      "scope": [
        "keyword"
      ],
      "settings": {
        "foreground": "#ff79c6"
      }
    },
    {
      "scope": [
        "entity.name.function",
        "support.function"
      ],
      "settings": {
        "foreground": "#ffb86c"
      }
    },
    {
      "scope": [
        "entity.name.class",
        "entity.name.type.class"
      ],
      "settings": {
        "foreground": "#bd93f9"
      }
    }
  ]
}