    "extensionButton.prominentBackground": "#33A6F0",
    "extensionButton.prominentForeground": "#FFFFFF",
    "extensionButton.prominentHoverBackground": "#2670BA",
    "pickerGroup.border": "#33A6F0",
    "pickerGroup.foreground": "#F0F0F0",
    "progressBar.background": "#33A6F0",
