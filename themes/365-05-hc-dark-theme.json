    "panelTitle.activeForeground": "#F0F0F0",
    "panelTitle.inactiveForeground": "#B0BFCF",
    "terminal.background": "#152238",
    "terminal.foreground": "#F0F0F0",
    "terminalCursor.foreground": "#94D248",
    "terminal.ansiBlack": "#152238",
    "terminal.ansiBlue": "#33A6F0",
