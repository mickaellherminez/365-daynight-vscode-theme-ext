    "terminal.ansiGreen": "#2AD881",
    "terminal.ansiMagenta": "#BB59CC",
    "terminal.ansiRed": "#FF6666",
    "terminal.ansiWhite": "#F0F0F0",
    "terminal.ansiYellow": "#FFD248",
    "button.background": "#33A6F0",
    "button.foreground": "#FFFFFF",
    "button.hoverBackground": "#2670BA",
    "dropdown.background": "#22364B",
    "dropdown.foreground": "#F0F0F0",
    "input.background": "#22364B",
    "input.foreground": "#F0F0F0",
    "input.placeholderForeground": "#B0BFCF",
    "scrollbarSlider.background": "#2A5370",
    "scrollbarSlider.hoverBackground": "#33A6F0",
