    "statusBar.background": "#143D59",
    "statusBar.foreground": "#E6FFFA",
    "statusBar.debuggingBackground": "#206A88",
    "statusBar.debuggingForeground": "#E6FFFA",
    "titleBar.activeBackground": "#143D59",
    "titleBar.activeForeground": "#E6FFFA",
    "titleBar.inactiveBackground": "#1B4F6B",
    "titleBar.inactiveForeground": "#C3F3EE",
    "panel.background": "#143D59",
    "panel.border": "#E6FFFA",
    "panelTitle.activeForeground": "#E6FFFA",
    "panelTitle.inactiveForeground": "#C3F3EE",
    "terminal.background": "#143D59",
    "terminal.foreground": "#E6FFFA",
    "terminalCursor.foreground": "#FFD700",
    "terminal.ansiBlack": "#143D59",
    "terminal.ansiRed": "#FF6347",
    "terminal.ansiGreen": "#32CD32",
    "terminal.ansiYellow": "#FFD700",
    "terminal.ansiBlue": "#1E90FF",
    "terminal.ansiMagenta": "#8A2BE2",
    "terminal.ansiCyan": "#00FFFF",
    "terminal.ansiWhite": "#E6FFFA",
    "terminal.ansiBrightBlack": "#C3F3EE",
    "terminal.ansiBrightRed": "#FF4500",
    "terminal.ansiBrightGreen": "#7CFC00",
    "terminal.ansiBrightYellow": "#FFFF00",
    "terminal.ansiBrightBlue": "#4682B4",
    "terminal.ansiBrightMagenta": "#DA70D6",
    "terminal.ansiBrightCyan": "#AFEEEE",
    "terminal.ansiBrightWhite": "#E6FFFA",
    "button.background": "#FFD700",
    "button.foreground": "#143D59",
    "button.hoverBackground": "#FFC000",
    "dropdown.background": "#1B4F6B",
    "dropdown.foreground": "#E6FFFA",
    "input.background": "#1B4F6B",
    "input.foreground": "#E6FFFA",
    "input.placeholderForeground": "#C3F3EE",
    "scrollbarSlider.background": "#245D73",
    "scrollbarSlider.hoverBackground": "#3F88A6",
    "scrollbarSlider.activeBackground": "#417EAF",
    "list.activeSelectionBackground": "#206A88",
    "list.activeSelectionForeground": "#E6FFFA",
    "list.inactiveSelectionBackground": "#1B4F6B",
