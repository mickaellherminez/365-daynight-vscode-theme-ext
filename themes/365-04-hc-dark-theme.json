    "tab.activeBackground": "#143D59",
    "tab.activeForeground": "#E6FFFA",
    "tab.inactiveBackground": "#1B4F6B",
    "tab.inactiveForeground": "#C3F3EE",
    "tab.border": "#E6FFFA",
    "activityBar.background": "#143D59",
    "activityBar.foreground": "#E6FFFA",
    "activityBar.inactiveForeground": "#C3F3EE",
    "activityBarBadge.background": "#FFD700",
    "activityBarBadge.foreground": "#143D59",
    "sideBar.background": "#1B4F6B",
    "sideBar.foreground": "#E6FFFA",
    "sideBarTitle.foreground": "#E6FFFA",
    "sideBarSectionHeader.background": "#206A88",
