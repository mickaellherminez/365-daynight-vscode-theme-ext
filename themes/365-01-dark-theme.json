{
  "name": "Thème Dark Janvier - Bleu Foncé",
  "type": "dark",
  "semanticHighlighting": true,
  "colors": {
    "editor.background": "#0D1B2A",
    "editor.foreground": "#E0E0E0",
    "editorLineNumber.foreground": "#577590",
    "editorCursor.foreground": "#F5CB5C",
    "editor.selectionBackground": "#1F4068",
    "editor.lineHighlightBackground": "#1B263B",
    "editorLineNumber.activeForeground": "#F5CB5C",
    "editorIndentGuide.background": "#1B263B",
    "editorIndentGuide.activeBackground": "#577590",
    "editorWhitespace.foreground": "#1B263B",
    "tab.activeBackground": "#1B263B",
    "tab.activeForeground": "#E0E0E0",
    "tab.inactiveBackground": "#0D1B2A",
    "tab.inactiveForeground": "#A0AFC0",
    "tab.border": "#1B263B",
    "activityBar.background": "#0D1B2A",
    "activityBar.foreground": "#E0E0E0",
    "activityBar.inactiveForeground": "#A0AFC0",
    "activityBarBadge.background": "#F5CB5C",
    "activityBarBadge.foreground": "#0D1B2A",
    "sideBar.background": "#0D1B2A",
    "sideBar.foreground": "#E0E0E0",
    "sideBarTitle.foreground": "#E0E0E0",
    "sideBarSectionHeader.background": "#1B263B",
    "sideBarSectionHeader.foreground": "#E0E0E0",
    "statusBar.background": "#0D1B2A",
    "statusBar.foreground": "#E0E0E0",
    "statusBar.debuggingBackground": "#1B263B",
    "statusBar.debuggingForeground": "#E0E0E0",
    "titleBar.activeBackground": "#0D1B2A",
    "titleBar.activeForeground": "#E0E0E0",
    "titleBar.inactiveBackground": "#0D1B2A",
    "titleBar.inactiveForeground": "#A0AFC0",
    "panel.background": "#0D1B2A",
    "panel.border": "#1B263B",
    "panelTitle.activeForeground": "#E0E0E0",
    "panelTitle.inactiveForeground": "#A0AFC0",
    "terminal.background": "#0D1B2A",
    "terminal.foreground": "#E0E0E0",
    "terminalCursor.foreground": "#F5CB5C",
    "terminal.ansiBlack": "#0D1B2A",
    "terminal.ansiBlue": "#1B98E0",
    "terminal.ansiBrightBlack": "#546E7A",
    "terminal.ansiBrightBlue": "#1B98E0",
    "terminal.ansiBrightCyan": "#00ADB5",
    "terminal.ansiBrightGreen": "#00A676",
    "terminal.ansiBrightMagenta": "#BB29BB",
    "terminal.ansiBrightRed": "#FF4B5C",
    "terminal.ansiBrightWhite": "#E0E0E0",
    "terminal.ansiBrightYellow": "#F5CB5C",
    "terminal.ansiCyan": "#00ADB5",
    "terminal.ansiGreen": "#00A676",
    "terminal.ansiMagenta": "#BB29BB",
    "terminal.ansiRed": "#FF4B5C",
    "terminal.ansiWhite": "#E0E0E0",
    "terminal.ansiYellow": "#F5CB5C",
    "button.background": "#1B98E0",
    "button.foreground": "#FFFFFF",
    "button.hoverBackground": "#166CA6",
    "dropdown.background": "#1B263B",
    "dropdown.foreground": "#E0E0E0",
    "input.background": "#1B263B",
    "input.foreground": "#E0E0E0",
    "input.placeholderForeground": "#A0AFC0",
    "scrollbarSlider.background": "#1F4068",
    "scrollbarSlider.hoverBackground": "#1B98E0",
    "scrollbarSlider.activeBackground": "#166CA6",
    "list.activeSelectionBackground": "#1B263B",
    "list.activeSelectionForeground": "#E0E0E0",
    "list.inactiveSelectionBackground": "#1B263B",
    "list.inactiveSelectionForeground": "#A0AFC0",
    "list.hoverBackground": "#1B263B",
    "list.hoverForeground": "#E0E0E0",
    "editorGroupHeader.tabsBackground": "#0D1B2A",
    "editorGroupHeader.tabsBorder": "#1B263B",
    "editorGroup.border": "#1B263B",
    "notification.background": "#1B263B",
    "notification.foreground": "#E0E0E0",
    "notification.buttonBackground": "#1B98E0",
    "notification.buttonForeground": "#FFFFFF",
    "notification.buttonHoverBackground": "#166CA6",
    "extensionButton.prominentBackground": "#1B98E0",
    "extensionButton.prominentForeground": "#FFFFFF",
    "extensionButton.prominentHoverBackground": "#166CA6",
    "pickerGroup.border": "#1B98E0",
    "pickerGroup.foreground": "#E0E0E0",
    "progressBar.background": "#1B98E0",
    "badge.background": "#1B98E0",
    "badge.foreground": "#FFFFFF",
    "menu.background": "#0D1B2A",
    "menu.foreground": "#E0E0E0",
    "menu.selectionBackground": "#1B263B",
    "menu.selectionForeground": "#E0E0E0",
    "menu.separatorBackground": "#1B263B",
    "menubar.selectionBackground": "#1B263B",
    "menubar.selectionForeground": "#E0E0E0"
  },
  "tokenColors": [
    {
      "name": "Commentaires",
      "scope": [
        "comment",
        "punctuation.definition.comment",
        "comment markup.link"
      ],
      "settings": {
        "foreground": "#a9a9a9",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Variables",
      "scope": [
        "variable",
        "variable.other.constant",
        "variable.language",
        "variable.parameter.function",
        "variable.parameter",
        "variable.other.readwrite",
        "variable.other.class",
        "variable.other.instance"
      ],
      "settings": {
        "foreground": "#56b4e9"
      }
    },
    {
      "name": "Constantes",
      "scope": [
        "constant",
        "constant.language",
        "constant.character",
        "constant.other",
        "support.constant",
        "support.constant.edge"
      ],
      "settings": {
        "foreground": "#e69f00"
      }
    },
    {
      "name": "Chaînes de caractères",
      "scope": [
        "string",
        "string.template",
        "punctuation.definition.string",
        "string.quoted.single",
        "string.quoted.double",
        "markup.inline.raw.string.markdown"
      ],
      "settings": {
        "foreground": "#f5cb5c"
      }
    },
    {
      "name": "Mots-clés",
      "scope": [
        "keyword",
        "keyword.control",
        "storage.type",
        "storage.modifier",
        "keyword.operator.expression.import",
        "keyword.operator.expression.instanceof",
        "keyword.operator.new",
        "keyword.operator.ternary",
        "keyword.operator.optional",
        "keyword.operator.expression.keyof"
      ],
      "settings": {
        "foreground": "#ff4b5c"
      }
    },
    {
      "name": "Fonctions",
      "scope": [
        "entity.name.function",
        "support.function",
        "meta.function-call",
        "variable.function",
        "support.function.std.rust",
        "support.function.console",
        "meta.function-call.generic.python"
      ],
      "settings": {
        "foreground": "#009e73"
      }
    },
    {
      "name": "Classes et Types",
      "scope": [
        "entity.name.type",
        "entity.name.class",
        "entity.name.namespace",
        "entity.name.scope-resolution",
        "support.class",
        "entity.other.inherited-class",
        "support.type",
        "support.type.property-name",
        "support.type.primitive.ts",
        "support.type.builtin.ts",
        "support.type.primitive.tsx",
        "support.type.builtin.tsx"
      ],
      "settings": {
        "foreground": "#bb29bb"
      }
    },
    {
      "name": "Méthodes",
      "scope": [
        "meta.method",
        "meta.method.declaration",
        "meta.method.java",
        "meta.method.groovy"
      ],
      "settings": {
        "foreground": "#1b98e0"
      }
    },
    {
      "name": "Nombres",
      "scope": [
        "constant.numeric",
        "constant.numeric.integer",
        "constant.numeric.float",
        "constant.numeric.hex",
        "constant.numeric.octal"
      ],
      "settings": {
        "foreground": "#f5cb5c"
      }
    },
    {
      "name": "Opérateurs",
      "scope": [
        "keyword.operator",
        "keyword.operator.assignment",
        "keyword.operator.arithmetic",
        "keyword.operator.logical",
        "keyword.operator.bitwise",
        "keyword.operator.channel",
        "keyword.operator.sizeof.c",
        "keyword.operator.sizeof.cpp",
        "keyword.operator.delete"
      ],
      "settings": {
        "foreground": "#e69f00"
      }
    },
    {
      "name": "Ponctuation",
      "scope": [
        "punctuation",
        "punctuation.definition",
        "punctuation.separator",
        "punctuation.terminator",
        "punctuation.definition.string.begin",
        "punctuation.definition.string.end",
        "punctuation.separator.key-value",
        "punctuation.separator.list.comma.css",
        "punctuation.section.block.begin.bracket.curly.cpp",
        "punctuation.section.block.end.bracket.curly.cpp"
      ],
      "settings": {
        "foreground": "#e0e0e0"
      }
    },
    {
      "name": "Balises HTML/XML",
      "scope": [
        "entity.name.tag",
        "punctuation.definition.tag",
        "meta.tag",
        "meta.tag.sgml",
        "markup.deleted.git_gutter"
      ],
      "settings": {
        "foreground": "#ff4b5c"
      }
    },
    {
      "name": "Attributs HTML/XML",
      "scope": [
        "entity.other.attribute-name",
        "entity.other.attribute-name.class.css",
        "entity.other.attribute-name.id.css"
      ],
      "settings": {
        "foreground": "#f5cb5c"
      }
    },
    {
      "name": "Support et modules",
      "scope": [
        "support.type",
        "support.class",
        "support.constant",
        "support.variable",
        "support.module.node",
        "support.type.object.module",
        "support.module.node",
        "entity.name.type.module"
      ],
      "settings": {
        "foreground": "#00adb5"
      }
    },
    {
      "name": "Markdown Titres",
      "scope": [
        "markup.heading",
        "punctuation.definition.heading",
        "entity.name.section.markdown",
        "markup.heading.setext",
        "markup.heading.setext.1.markdown",
        "markup.heading.setext.2.markdown"
      ],
      "settings": {
        "foreground": "#ff4b5c",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markdown Emphase",
      "scope": [
        "markup.italic",
        "punctuation.definition.italic",
        "emphasis md",
        "markup.italic.markdown"
      ],
      "settings": {
        "foreground": "#f5cb5c",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Markdown Gras",
      "scope": [
        "markup.bold",
        "punctuation.definition.bold",
        "punctuation.definition.bold.markdown"
      ],
      "settings": {
        "foreground": "#e69f00",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markdown Liens",
      "scope": [
        "markup.underline.link",
        "string.other.link",
        "markup.underline.link.markdown",
        "markup.underline.link.image.markdown"
      ],
      "settings": {
        "foreground": "#1b98e0"
      }
    },
    {
      "name": "Markdown Code",
      "scope": [
        "markup.fenced_code",
        "markup.inline.raw",
        "markup.inline.raw.markdown",
        "markup.inline.raw.string.markdown"
      ],
      "settings": {
        "foreground": "#56b4e9"
      }
    },
    {
      "name": "Markdown Listes",
      "scope": [
        "markup.list",
        "punctuation.definition.list",
        "punctuation.definition.list.begin.markdown",
        "punctuation.definition.list.markdown"
      ],
      "settings": {
        "foreground": "#f5cb5c"
      }
    },
    {
      "name": "Markdown Citations",
      "scope": [
        "markup.quote",
        "markup.quote.markdown"
      ],
      "settings": {
        "foreground": "#a9a9a9",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Caractères spéciaux",
      "scope": [
        "constant.character.escape",
        "constant.character.escape.markdown"
      ],
      "settings": {
        "foreground": "#ff4b5c"
      }
    },
    {
      "name": "Stockage",
      "scope": [
        "storage",
        "token.storage",
        "token.storage.type.java"
      ],
      "settings": {
        "foreground": "#bb29bb"
      }
    },
    {
      "name": "Entités",
      "scope": [
        "entity.other.inherited-class",
        "entity.global.clojure",
        "meta.definition.class.inherited.classes.groovy"
      ],
      "settings": {
        "foreground": "#009e73"
      }
    },
    {
      "name": "Propriétés JSON",
      "scope": [
        "support.type.property-name.json",
        "support.type.property-name.json punctuation"
      ],
      "settings": {
        "foreground": "#1b98e0"
      }
    },
    {
      "name": "Expressions régulières",
      "scope": [
        "string.regexp",
        "constant.other.character-class.regexp",
        "keyword.operator.quantifier.regexp"
      ],
      "settings": {
        "foreground": "#e69f00"
      }
    },
    {
      "name": "Invalide",
      "scope": [
        "invalid",
        "invalid.illegal",
        "invalid.broken",
        "invalid.deprecated",
        "invalid.unimplemented"
      ],
      "settings": {
        "foreground": "#ff4b5c",
        "fontStyle": "underline"
      }
    },
    {
      "name": "Déprécié",
      "scope": [
        "invalid.deprecated"
      ],
      "settings": {
        "foreground": "#bb29bb",
        "fontStyle": "strikethrough"
      }
    },
    {
      "name": "JSON Punctuation for Property Name",
      "scope": [
        "support.type.property-name.json punctuation"
      ],
      "settings": {
        "foreground": "#1b98e0"
      }
    },
    {
      "name": "JSON String Value",
      "scope": [
        "source.json meta.structure.dictionary.json > value.json > string.quoted.json",
        "source.json meta.structure.array.json > value.json > string.quoted.json"
      ],
      "settings": {
        "foreground": "#f5cb5c"
      }
    },
    {
      "name": "JSON Key",
      "scope": [
        "source.json meta.structure.dictionary.json > string.quoted.json"
      ],
      "settings": {
        "foreground": "#1b98e0"
      }
    },
    {
      "name": "JSON Punctuation",
      "scope": [
        "source.json meta.structure.dictionary.json > string.quoted.json > punctuation.string",
        "source.json meta.structure.dictionary.json > value.json > string.quoted.json > punctuation",
        "source.json meta.structure.array.json > value.json > string.quoted.json > punctuation"
      ],
      "settings": {
        "foreground": "#f5cb5c"
      }
    }
  ]
}