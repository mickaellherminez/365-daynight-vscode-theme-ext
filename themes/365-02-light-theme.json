    "tab.border": "#CFD8DC",
    "activityBar.background": "#FFFFFF",
    "activityBar.foreground": "#1A1A1A",
    "activityBar.inactiveForeground": "#3B4252",
    "activityBarBadge.background": "#D32F2F",
    "activityBarBadge.foreground": "#FFFFFF",
    "sideBar.background": "#F5F5F5",
