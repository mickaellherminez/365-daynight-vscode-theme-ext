    "panel.background": "#FFFFFF",
    "panel.border": "#CFD8DC",
    "panelTitle.activeForeground": "#1A1A1A",
    "panelTitle.inactiveForeground": "#3B4252",
    "terminal.background": "#FFFFFF",
    "terminal.foreground": "#1A1A1A",
    "terminalCursor.foreground": "#D32F2F",
    "terminal.ansiBlack": "#263238",
    "terminal.ansiRed": "#D32F2F",
    "terminal.ansiGreen": "#388E3C",
    "terminal.ansiYellow": "#F57F17",
    "terminal.ansiBlue": "#1976D2",
