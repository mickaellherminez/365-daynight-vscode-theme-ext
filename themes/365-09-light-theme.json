  "tokenColors": [
    {
      "name": "Comments",
      "scope": [
        "comment",
        "punctuation.definition.comment"
      ],
      "settings": {
        "foreground": "#90A4AE"
      }
    },
    {
      "name": "Keywords",
      "scope": [
        "keyword",
        "keyword.control",
        "storage.type",
        "storage.modifier"
      ],
      "settings": {
        "foreground": "#FF8F00"
      }
    },
    {
      "name": "Variables",
      "scope": [
        "variable",
        "string constant.other.placeholder",
        "variable.other.constant",
        "variable.language"
      ],
      "settings": {
        "foreground": "#00ACC1"
      }
    },
    {
      "name": "Strings",
      "scope": [
        "string",
        "string.template",
        "punctuation.definition.string",
        "string.quoted.single",
        "string.quoted.double"
      ],
      "settings": {
        "foreground": "#43A047"
      }
    },
    {
      "name": "Functions",
      "scope": [
        "entity.name.function",
        "support.function",
        "meta.function-call",
        "variable.function"
      ],
      "settings": {
        "foreground": "#1E88E5"
      }
    },
    {
      "name": "Classes",
      "scope": [
        "entity.name.type",
        "entity.name.class",
        "entity.name.namespace",
        "entity.name.scope-resolution",
        "support.class"
      ],
      "settings": {
        "foreground": "#E53935"
      }
    },
    {
      "name": "Markdown Emphasis",
      "scope": [
        "markup.italic",
        "punctuation.definition.italic"
      ],
      "settings": {
        "foreground": "#43A047"
      }
    },
    {
      "name": "Markdown Headers",
      "scope": [
        "markup.heading",
        "punctuation.definition.heading"
      ],
      "settings": {
        "foreground": "#1E88E5",
        "fontStyle": "bold"
      }
    }
  ]
}