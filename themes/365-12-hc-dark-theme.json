  "tokenColors": [
    {
      "name": "Commentaires",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "foreground": "#839B97",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Variables",
      "scope": [
        "variable",
        "string constant.other.placeholder",
        "variable.other.constant",
        "variable.language"
      ],
      "settings": {
        "foreground": "#2E9CCA"
      }
    },
    {
      "name": "Chaînes de caractères",
      "scope": [
        "string",
        "string.template",
        "punctuation.definition.string",
        "string.quoted.single",
        "string.quoted.double"
      ],
      "settings": {
        "foreground": "#FF5F5D"
      }
    },
    {
      "name": "Mots-clés",
      "scope": [
        "keyword",
        "keyword.control",
        "storage.type",
        "storage.modifier"
      ],
      "settings": {
        "foreground": "#B1E1FF"
      }
    },
    {
      "name": "Fonctions",
      "scope": [
        "entity.name.function",
        "support.function",
        "meta.function-call",
        "variable.function"
      ],
      "settings": {
        "foreground": "#B1E1FF"
      }
    },
    {
      "name": "Classes",
      "scope": [
        "entity.name.type",
        "entity.name.class",
        "entity.name.namespace",
        "entity.name.scope-resolution",
        "support.class"
      ],
      "settings": {
        "foreground": "#ADADFF"
      }
    },
    {
      "name": "Méthodes",
      "scope": ["meta.method", "meta.method.declaration"],
      "settings": {
        "foreground": "#B1E1FF"
      }
    },
    {
      "name": "Constantes",
      "scope": [
        "constant",
        "constant.language",
        "constant.character",
        "constant.other"
      ],
      "settings": {
        "foreground": "#2E9CCA"
      }
    },
    {
      "name": "Nombres",
      "scope": [
        "constant.numeric",
        "constant.numeric.integer",
        "constant.numeric.float",
        "constant.numeric.hex",
        "constant.numeric.octal"
      ],
      "settings": {
        "foreground": "#2E9CCA"
      }
    },
    {
      "name": "Opérateurs",
      "scope": [
        "keyword.operator",
        "keyword.operator.assignment",
        "keyword.operator.arithmetic",
        "keyword.operator.logical"
      ],
      "settings": {
        "foreground": "#FF3E41"
      }
    },
    {
      "name": "Ponctuation",
      "scope": [
        "punctuation",
        "punctuation.definition",
        "punctuation.separator",
        "punctuation.terminator"
      ],
      "settings": {
        "foreground": "#E3FDFD"
      }
    },
    {
      "name": "Balises HTML/XML",
      "scope": ["entity.name.tag", "punctuation.definition.tag"],
      "settings": {
        "foreground": "#2E9CCA"
      }
    },
    {
      "name": "Attributs HTML/XML",
      "scope": ["entity.other.attribute-name"],
      "settings": {
        "foreground": "#B1E1FF"
      }
    },
    {
      "name": "Support",
      "scope": [
        "support.type",
        "support.class",
        "support.constant",
        "support.variable"
      ],
      "settings": {
        "foreground": "#ADADFF"
      }
    },
    {
      "name": "Markdown Titres",
      "scope": ["markup.heading", "punctuation.definition.heading"],
      "settings": {
        "foreground": "#2E9CCA",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markdown Emphase",
      "scope": ["markup.italic", "punctuation.definition.italic"],
      "settings": {
        "foreground": "#FF5F5D",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Markdown Gras",
      "scope": ["markup.bold", "punctuation.definition.bold"],
      "settings": {
        "foreground": "#B1E1FF",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markdown Liens",
      "scope": ["markup.underline.link", "string.other.link"],
      "settings": {
        "foreground": "#2E9CCA"
      }
    },
    {
      "name": "Markdown Code",
      "scope": ["markup.fenced_code", "markup.inline.raw"],
      "settings": {
        "foreground": "#3CC47C"
      }
    }
  ],
  "semanticTokenColors": {
    "variable.readonly": "#3CC47C",
    "parameter": "#2E9CCA",
    "function": "#B1E1FF",
    "method": "#B1E1FF",
    "class": "#ADADFF",
    "enum": "#B1E1FF",
    "interface": "#ADADFF",
    "namespace": "#839B97",
    "typeParameter": "#2E9CCA",
    "enumMember": "#2E9CCA",
    "property": "#E3FDFD",
    "variable.defaultLibrary": "#FF3E41",
    "keyword.operator": "#FF3E41",
    "comment.documentation": {
      "foreground": "#839B97",
      "fontStyle": "italic"
    }
  }
}
