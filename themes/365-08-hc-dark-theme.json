  "tokenColors": [
    {
      "name": "Commentaires",
      "scope": [
        "comment",
        "punctuation.definition.comment"
      ],
      "settings": {
        "foreground": "#888888",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Variables",
      "scope": [
        "variable",
        "string constant.other.placeholder",
        "variable.other.constant",
        "variable.language"
      ],
      "settings": {
        "foreground": "#00FF00"
      }
    },
    {
      "name": "Chaînes de caractères",
      "scope": [
        "string",
        "string.template",
        "punctuation.definition.string",
        "string.quoted.single",
        "string.quoted.double"
      ],
      "settings": {
        "foreground": "#FFFF00"
      }
    },
    {
      "name": "Mots-clés",
      "scope": [
        "keyword",
        "keyword.control",
        "storage.type",
        "storage.modifier"
      ],
      "settings": {
        "foreground": "#00FFFF"
      }
    },
    {
      "name": "Fonctions",
      "scope": [
        "entity.name.function",
        "support.function",
        "meta.function-call",
        "variable.function",
        "keyword.other.special-method"
      ],
      "settings": {
        "foreground": "#FF00FF"
      }
    },
    {
      "name": "Classes",
      "scope": [
        "entity.name.type",
        "entity.name.class",
        "entity.name.namespace",
        "entity.name.scope-resolution",
        "support.class"
      ],
      "settings": {
        "foreground": "#FFA500"
      }
    },
    {
      "name": "Méthodes",
      "scope": [
        "meta.method",
        "meta.method.declaration"
      ],
      "settings": {
        "foreground": "#FF00FF"
      }
    },
    {
      "name": "Constantes",
      "scope": [
        "constant",
        "constant.language",
        "constant.character",
        "constant.numeric",
        "constant.other"
      ],
      "settings": {
        "foreground": "#FF0000"
      }
    },
    {
      "name": "Nombres",
      "scope": [
        "constant.numeric",
        "constant.numeric.integer",
        "constant.numeric.float",
        "constant.numeric.hex",
        "constant.numeric.octal",
        "constant.numeric.binary"
      ],
      "settings": {
        "foreground": "#FF0000"
      }
    },
    {
      "name": "Opérateurs",
      "scope": [
        "keyword.operator",
        "keyword.operator.assignment",
        "keyword.operator.arithmetic",
        "keyword.operator.bitwise",
        "keyword.operator.logical",
        "keyword.operator.comparison",
        "keyword.operator.increment-decrement",
        "keyword.operator.ternary",
        "keyword.operator.relational",
        "keyword.operator.misc"
      ],
      "settings": {
        "foreground": "#FFFFFF"
      }
    },
    {
      "name": "Ponctuation",
      "scope": [
        "punctuation",
        "punctuation.definition",
        "punctuation.separator",
        "punctuation.terminator"
      ],
      "settings": {
        "foreground": "#FFFFFF"
      }
    },
    {
      "name": "Balises HTML/XML",
      "scope": [
        "entity.name.tag",
        "punctuation.definition.tag"
      ],
      "settings": {
        "foreground": "#FFA500"
      }
    },
    {
      "name": "Attributs HTML/XML",
      "scope": [
        "entity.other.attribute-name"
      ],
      "settings": {
        "foreground": "#00FF00"
      }
    },
    {
      "name": "Support",
      "scope": [
        "support.type",
        "support.class",
        "support.constant",
        "support.variable",
        "support.other.variable",
        "support.function"
      ],
      "settings": {
        "foreground": "#00FFFF"
      }
    },
    {
      "name": "Markdown Titres",
      "scope": [
        "markup.heading",
        "entity.name.section",
        "punctuation.definition.heading"
      ],
      "settings": {
        "foreground": "#00FF00",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markdown Emphase",
      "scope": [
        "markup.italic",
        "punctuation.definition.italic"
      ],
      "settings": {
        "foreground": "#FFFF00",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Markdown Gras",
      "scope": [
        "markup.bold",
        "punctuation.definition.bold"
      ],
      "settings": {
        "foreground": "#00FF00",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markdown Liens",
      "scope": [
        "markup.underline.link",
        "string.other.link"
      ],
      "settings": {
        "foreground": "#0000FF",
        "fontStyle": "underline"
      }
    },
    {
      "name": "Markdown Code",
      "scope": [
        "markup.fenced_code.block",
        "markup.inline.raw",
        "markup.raw.block"
      ],
      "settings": {
        "foreground": "#FF00FF",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Caractères spéciaux",
      "scope": [
        "constant.character.escape"
      ],
      "settings": {
        "foreground": "#FFFFFF",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Stockage",
      "scope": [
        "storage",
        "storage.type",
        "storage.modifier"
      ],
      "settings": {
        "foreground": "#00FFFF"
      }
    },
    {
      "name": "Entités",
      "scope": [
        "entity.other.inherited-class"
      ],
      "settings": {
        "foreground": "#FFA500"
      }
    },
    {
      "name": "JSON Keys",
      "scope": "support.type.property-name.json",
      "settings": {
        "foreground": "#00FF00"
      }
    },
    {
      "name": "YAML Keys",
      "scope": "entity.name.tag.yaml",
      "settings": {
        "foreground": "#00FF00"
      }
    },
    {
      "name": "Diff Ajouté",
      "scope": "markup.inserted",
      "settings": {
        "foreground": "#00FF00",
        "background": "#003300"
      }
    },
    {
      "name": "Diff Supprimé",
      "scope": "markup.deleted",
      "settings": {
        "foreground": "#FF0000",
        "background": "#330000"
      }
    },
    {
      "name": "Diff Modifié",
      "scope": "markup.changed",
      "settings": {
        "foreground": "#FFFF00",
        "background": "#333300"
      }
    }
  ]
}
