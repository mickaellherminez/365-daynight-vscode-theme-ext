  "tokenColors": [
    {
      "name": "Commentaires",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "foreground": "#6A8759"
      }
    },
    {
      "name": "Variables",
      "scope": [
        "variable",
        "string constant.other.placeholder",
        "variable.other.constant",
        "variable.language"
      ],
      "settings": {
        "foreground": "#9876AA"
      }
    },
    {
      "name": "Chaînes de caractères",
      "scope": [
        "string",
        "string.template",
        "punctuation.definition.string",
        "string.quoted.single",
        "string.quoted.double"
      ],
      "settings": {
        "foreground": "#629755"
      }
    },
    {
      "name": "Mots-clés",
      "scope": [
        "keyword",
        "keyword.control",
        "storage.type",
        "storage.modifier"
      ],
      "settings": {
        "foreground": "#CC7832"
      }
    },
    {
      "name": "Fonctions",
      "scope": [
        "entity.name.function",
        "support.function",
        "meta.function-call",
        "variable.function"
      ],
      "settings": {
        "foreground": "#A9B7C6"
      }
    },
    {
      "name": "Classes",
      "scope": [
        "entity.name.type",
        "entity.name.class",
        "entity.name.namespace",
        "entity.name.scope-resolution",
        "support.class"
      ],
      "settings": {
        "foreground": "#287BDE"
      }
    },
    {
      "name": "Méthodes",
      "scope": ["meta.method", "meta.method.declaration"],
      "settings": {
        "foreground": "#FFC66D"
      }
    },
    {
      "name": "Constantes",
      "scope": [
        "constant",
        "constant.language",
        "constant.character",
        "constant.other"
      ],
      "settings": {
        "foreground": "#9876AA"
      }
    },
    {
      "name": "Nombres",
      "scope": [
        "constant.numeric",
        "constant.numeric.integer",
        "constant.numeric.float",
        "constant.numeric.hex",
        "constant.numeric.octal"
      ],
      "settings": {
        "foreground": "#6897BB"
      }
    },
    {
      "name": "Opérateurs",
      "scope": [
        "keyword.operator",
        "keyword.operator.assignment",
        "keyword.operator.arithmetic",
        "keyword.operator.logical"
      ],
      "settings": {
        "foreground": "#CC7832"
      }
    },
    {
      "name": "Ponctuation",
      "scope": [
        "punctuation",
        "punctuation.definition",
        "punctuation.separator",
        "punctuation.terminator"
      ],
      "settings": {
        "foreground": "#A9B7C6"
      }
    },
    {
      "name": "Balises HTML/XML",
      "scope": ["entity.name.tag", "punctuation.definition.tag"],
      "settings": {
        "foreground": "#E8BF6A"
      }
    },
    {
      "name": "Attributs HTML/XML",
      "scope": ["entity.other.attribute-name"],
      "settings": {
        "foreground": "#A5C261"
      }
    },
    {
      "name": "Support",
      "scope": [
        "support.type",
        "support.class",
        "support.constant",
        "support.variable"
      ],
      "settings": {
        "foreground": "#B0C4DE"
      }
    },
    {
      "name": "Markdown Titres",
      "scope": ["markup.heading", "punctuation.definition.heading"],
      "settings": {
        "foreground": "#CC7832",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markdown Emphase",
      "scope": ["markup.italic", "punctuation.definition.italic"],
      "settings": {
        "foreground": "#A5C261"
      }
    },
    {
      "name": "Markdown Gras",
      "scope": ["markup.bold", "punctuation.definition.bold"],
      "settings": {
        "foreground": "#E8BF6A",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markdown Liens",
      "scope": ["markup.underline.link", "string.other.link"],
      "settings": {
        "foreground": "#287BDE"
      }
    },
    {
      "name": "Markdown Code",
      "scope": ["markup.fenced_code", "markup.inline.raw"],
      "settings": {
        "foreground": "#629755"
      }
    },
    {
      "name": "Caractères spéciaux",
      "scope": ["constant.character.escape"],
      "settings": {
        "foreground": "#CC7832"
      }
    },
    {
      "name": "Stockage",
      "scope": ["storage"],
      "settings": {
        "foreground": "#CC7832"
      }
    },
    {
      "name": "Entités",
      "scope": ["entity.other.inherited-class"],
      "settings": {
        "foreground": "#287BDE"
      }
    }
  ]
}
