  "tokenColors": [
    {
      "name": "Commentaires",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "foreground": "#8E6C53",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Variables",
      "scope": [
        "variable",
        "string constant.other.placeholder",
        "variable.other.constant",
        "variable.language"
      ],
      "settings": {
        "foreground": "#FF7F50"
      }
    },
    {
      "name": "Chaînes de caractères",
      "scope": [
        "string",
        "string.template",
        "punctuation.definition.string",
        "string.quoted.single",
        "string.quoted.double"
      ],
      "settings": {
        "foreground": "#D4A15D"
      }
    },
    {
      "name": "Mots-clés",
      "scope": [
        "keyword",
        "keyword.control",
        "storage.type",
        "storage.modifier"
      ],
      "settings": {
        "foreground": "#B8860B"
      }
    },
    {
      "name": "Fonctions",
      "scope": [
        "entity.name.function",
        "support.function",
        "meta.function-call",
        "variable.function"
      ],
      "settings": {
        "foreground": "#D2691E"
      }
    },
    {
      "name": "Classes",
      "scope": [
        "entity.name.type",
        "entity.name.class",
        "entity.name.namespace",
        "entity.name.scope-resolution",
        "support.class"
      ],
      "settings": {
        "foreground": "#A0522D"
      }
    },
    {
      "name": "Méthodes",
      "scope": ["meta.method", "meta.method.declaration"],
      "settings": {
        "foreground": "#D2691E"
      }
    },
    {
      "name": "Constantes",
      "scope": [
        "constant",
        "constant.language",
        "constant.character",
        "constant.other"
      ],
      "settings": {
        "foreground": "#FFD700"
      }
    },
    {
      "name": "Nombres",
      "scope": [
        "constant.numeric",
        "constant.numeric.integer",
        "constant.numeric.float",
        "constant.numeric.hex",
        "constant.numeric.octal"
      ],
      "settings": {
        "foreground": "#FFD700"
      }
    },
    {
      "name": "Opérateurs",
      "scope": [
        "keyword.operator",
        "keyword.operator.assignment",
        "keyword.operator.arithmetic",
        "keyword.operator.logical"
      ],
      "settings": {
        "foreground": "#8B0000"
      }
    },
    {
      "name": "Ponctuation",
      "scope": [
        "punctuation",
        "punctuation.definition",
        "punctuation.separator",
        "punctuation.terminator"
      ],
      "settings": {
        "foreground": "#E0D7C6"
      }
    },
    {
      "name": "Balises HTML/XML",
      "scope": ["entity.name.tag", "punctuation.definition.tag"],
      "settings": {
        "foreground": "#FF7F50"
      }
    },
    {
      "name": "Attributs HTML/XML",
      "scope": ["entity.other.attribute-name"],
      "settings": {
        "foreground": "#B8860B"
      }
    },
    {
      "name": "Support",
      "scope": [
        "support.type",
        "support.class",
        "support.constant",
        "support.variable"
      ],
      "settings": {
        "foreground": "#A0522D"
      }
    },
    {
      "name": "Markdown Titres",
      "scope": ["markup.heading", "punctuation.definition.heading"],
      "settings": {
        "foreground": "#FF7F50",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markdown Emphase",
      "scope": ["markup.italic", "punctuation.definition.italic"],
      "settings": {
        "foreground": "#D4A15D",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Markdown Gras",
      "scope": ["markup.bold", "punctuation.definition.bold"],
      "settings": {
        "foreground": "#B8860B",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markdown Liens",
      "scope": ["markup.underline.link", "string.other.link"],
      "settings": {
        "foreground": "#FFD700"
      }
    },
    {
      "name": "Markdown Code",
      "scope": ["markup.fenced_code", "markup.inline.raw"],
      "settings": {
        "foreground": "#14A460"
      }
    }
  ]
}