  "tokenColors": [
    {
      "name": "Comments",
      "scope": [
        "comment",
        "punctuation.definition.comment"
      ],
      "settings": {
        "foreground": "#6C758F"
      }
    },
    {
      "name": "Keywords",
      "scope": [
        "keyword",
        "keyword.control",
        "storage.type",
        "storage.modifier"
      ],
      "settings": {
        "foreground": "#FFA42C"
      }
    },
    {
      "name": "Variables",
      "scope": [
        "variable",
        "string constant.other.placeholder",
        "variable.other.constant",
        "variable.language"
      ],
      "settings": {
        "foreground": "#74A4CF"
      }
    },
    {
      "name": "Strings",
      "scope": [
        "string",
        "string.template",
        "punctuation.definition.string",
        "string.quoted.single",
        "string.quoted.double"
      ],
      "settings": {
        "foreground": "#8EB06E"
      }
    },
    {
      "name": "Functions",
      "scope": [
        "entity.name.function",
        "support.function",
        "meta.function-call",
        "variable.function"
      ],
      "settings": {
        "foreground": "#6D91C3"
      }
    },
    {
      "name": "Classes",
      "scope": [
        "entity.name.type",
        "entity.name.class",
        "entity.name.namespace",
        "entity.name.scope-resolution",
        "support.class"
      ],
      "settings": {
        "foreground": "#AC8BAE"
      }
    },
    {
      "name": "Constants",
      "scope": [
        "constant.numeric",
        "constant.language.boolean"
      ],
      "settings": {
        "foreground": "#E6BF6D"
      }
    },
    {
      "name": "Operators",
      "scope": [
        "keyword.operator",
        "keyword.operator.arithmetic",
        "keyword.operator.logical"
      ],
      "settings": {
        "foreground": "#D08770"
      }
    },
    {
      "name": "Documentation Comments",
      "scope": [
        "comment.documentation",
        "punctuation.definition.comment.documentation"
      ],
      "settings": {
        "foreground": "#6C758F"
      }
    },
    {
      "name": "Nombres",
      "scope": [
        "constant.numeric",
        "constant.numeric.integer",
        "constant.numeric.float",
        "constant.numeric.hex",
        "constant.numeric.octal"
      ],
      "settings": {
        "foreground": "#E6BF6D"
      }
    },
    {
      "name": "Ponctuation",
      "scope": [
        "punctuation",
        "punctuation.definition",
        "punctuation.separator",
        "punctuation.terminator"
      ],
      "settings": {
        "foreground": "#EBECF2"
      }
    },
    {
      "name": "Balises HTML/XML",
      "scope": [
        "entity.name.tag",
        "punctuation.definition.tag"
      ],
      "settings": {
        "foreground": "#6D91C3"
      }
    },
    {
      "name": "Attributs HTML/XML",
      "scope": [
        "entity.other.attribute-name"
      ],
      "settings": {
        "foreground": "#E6BF6D"
      }
    },
    {
      "name": "Support",
      "scope": [
        "support.type",
        "support.class",
        "support.constant",
        "support.variable"
      ],
      "settings": {
        "foreground": "#FFA42C"
      }
    },
    {
      "name": "Titres Markdown",
      "scope": [
        "markup.heading",
        "punctuation.definition.heading"
      ],
      "settings": {
        "foreground": "#AC8BAE",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Emphase Markdown",
      "scope": [
        "markup.italic",
        "punctuation.definition.italic"
      ],
      "settings": {
        "foreground": "#E6BF6D"
      }
    },
    {
      "name": "Gras Markdown",
      "scope": [
        "markup.bold",
        "punctuation.definition.bold"
      ],
      "settings": {
        "foreground": "#FFA42C",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Liens Markdown",
      "scope": [
        "markup.underline.link",
        "string.other.link"
      ],
      "settings": {
        "foreground": "#6D91C3",
        "fontStyle": "underline"
      }
    },
    {
      "name": "Code Markdown",
      "scope": [
        "markup.fenced_code",
        "markup.inline.raw"
      ],
      "settings": {
        "foreground": "#8EB06E"
      }
    },
    {
      "name": "Caractères d'échappement",
      "scope": [
        "constant.character.escape"
      ],
      "settings": {
        "foreground": "#C05E5E"
      }
    },
    {
      "name": "Stockage",
      "scope": [
        "storage"
      ],
      "settings": {
        "foreground": "#E6BF6D"
      }
    },
    {
      "name": "Entités",
      "scope": [
        "entity.other.inherited-class"
      ],
      "settings": {
        "foreground": "#AC8BAE"
      }
    },
    {
      "name": "Clés JSON",
      "scope": "support.type.property-name.json",
      "settings": {
        "foreground": "#E6BF6D"
      }
    },
    {
      "name": "Classes CSS",
      "scope": "entity.other.attribute-name.class.css",
      "settings": {
        "foreground": "#6D91C3"
      }
    },
    {
      "name": "IDs CSS",
      "scope": "entity.other.attribute-name.id.css",
      "settings": {
        "foreground": "#8EB06E"
      }
    },
    {
      "name": "Expressions régulières",
      "scope": "string.regexp",
      "settings": {
        "foreground": "#AC8BAE"
      }
    },
    {
      "name": "Constantes de langage",
      "scope": "variable.language",
      "settings": {
        "foreground": "#E6BF6D"
      }
    },
    {
      "name": "Fonctions de bibliothèque",
      "scope": "support.function",
      "settings": {
        "foreground": "#6D91C3"
      }
    },
    {
      "name": "Constantes de bibliothèque",
      "scope": "support.constant",
      "settings": {
        "foreground": "#FFA42C"
      }
    }
  ],
