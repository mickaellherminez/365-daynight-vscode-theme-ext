  "tokenColors": [
    {
      "name": "Comments",
      "scope": [
        "comment",
        "punctuation.definition.comment"
      ],
      "settings": {
        "foreground": "#667488"
      }
    },
    {
      "name": "Keywords",
      "scope": [
        "keyword",
        "keyword.control",
        "storage.type",
        "storage.modifier"
      ],
      "settings": {
        "foreground": "#FF8800"
      }
    },
    {
      "name": "Variables",
      "scope": [
        "variable",
        "string constant.other.placeholder",
        "variable.other.constant",
        "variable.language"
      ],
      "settings": {
        "foreground": "#88C0D0"
      }
    },
    {
      "name": "Strings",
      "scope": [
        "string",
        "string.template",
        "punctuation.definition.string",
        "string.quoted.single",
        "string.quoted.double"
      ],
      "settings": {
        "foreground": "#A3BE8C"
      }
    },
    {
      "name": "Functions",
      "scope": [
        "entity.name.function",
        "support.function",
        "meta.function-call",
        "variable.function"
      ],
      "settings": {
        "foreground": "#70A4DC"
      }
    },
    {
      "name": "Classes",
      "scope": [
        "entity.name.type",
        "entity.name.class",
        "entity.name.namespace",
        "entity.name.scope-resolution",
        "support.class"
      ],
      "settings": {
        "foreground": "#B48EAD"
      }
    },
    {
      "name": "Markdown Emphasis",
      "scope": [
        "markup.italic",
        "punctuation.definition.italic"
      ],
      "settings": {
        "foreground": "#A3BE8C"
      }
    },
    {
      "name": "Markdown Headers",
      "scope": [
        "markup.heading",
        "punctuation.definition.heading"
      ],
      "settings": {
        "foreground": "#70A4DC",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Numbers",
      "scope": [
        "constant.numeric",
        "constant.numeric.integer",
        "constant.numeric.float",
        "constant.numeric.hex",
        "constant.numeric.octal"
      ],
      "settings": {
        "foreground": "#EBCB8B"
      }
    },
    {
      "name": "Operators",
      "scope": [
        "keyword.operator",
        "keyword.operator.assignment",
        "keyword.operator.arithmetic",
        "keyword.operator.logical"
      ],
      "settings": {
        "foreground": "#D08770"
      }
    }
  ]
}