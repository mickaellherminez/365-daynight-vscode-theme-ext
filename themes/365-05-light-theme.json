    "extensionButton.prominentBackground": "#F39C12",
    "extensionButton.prominentForeground": "#FDFEFE",
    "extensionButton.prominentHoverBackground": "#F5B041",
    "pickerGroup.border": "#F39C12",
    "pickerGroup.foreground": "#2C3E50",
    "progressBar.background": "#F39C12",
    "badge.background": "#F39C12",
    "badge.foreground": "#FDFEFE",
    "menu.background": "#FDFEFE",
    "menu.foreground": "#2C3E50",
