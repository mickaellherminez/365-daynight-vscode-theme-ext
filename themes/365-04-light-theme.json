    "terminal.ansiBrightBlue": "#64B5F6",
    "terminal.ansiBrightMagenta": "#CE93D8",
    "terminal.ansiBrightCyan": "#4DD0E1",
    "terminal.ansiBrightWhite": "#FFFFFF",
    "button.background": "#FFA726",
    "button.foreground": "#FFF8E1",
