  "tokenColors": [
    {
      "name": "Commentaires",
      "scope": [
        "comment",
        "punctuation.definition.comment"
      ],
      "settings": {
        "foreground": "#839B97",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Variables",
      "scope": [
